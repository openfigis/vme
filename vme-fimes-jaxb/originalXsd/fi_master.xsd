		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeartypeAccEquip" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geartype Accessory Equipment</Title>
				<Description>GeartypeAccEquip contains information about accessory equipment for a gear type.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
			<xs:element name="FishTechniqueRef" type="fi:FishTechniqueRef"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeartypeChild" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geartype Child</Title>
				<Description>GeartypeChild is a container for creating new geartype groups. To define the group using existing geartypes use fi:GeartypeRef. To define the group by creating new geartypes use fi:Geartype.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Geartype" type="fi:Geartype"/>
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeartypeEnviron" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geartype Environment</Title>
				<Description>GeartypeEnviron contains information on a gear's environment.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="FishingEnviron" type="fi:FishingEnviron"/>
			<xs:element name="Impacts" type="fi:Impacts"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeartypeHandlingEquip" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geartype Handling Equipment</Title>
				<Description>GeartypeHandlingEquip contains information about gear handling equipment for a gear type.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
			<xs:element name="FishTechniqueRef" type="fi:FishTechniqueRef"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeartypeHierarchy" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geartype Hierarchy</Title>
				<Description>contains names and levels of associated Geartypes.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="Level" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Top"/>
					<xs:enumeration value="Second"/>
					<xs:enumeration value="Third"/>
					<xs:enumeration value="Local"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeartypeIdent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geartype Identity</Title>
				<Description>GeartypeIdent contains the geartype classification codes necessary for positive identification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:element ref="dc:Title" maxOccurs="3"/>
				<xs:choice maxOccurs="unbounded">
					<xs:element name="ISSCFGCode" type="fi:ISSCFGCode"/>
					<!--	<xs:element name="GeartypeParent" type="fi:GeartypeParent"/>
					<xs:element name="VesseltypeHierarchy" type="fi:VesseltypeHierarchy"/>-->
					<xs:element name="Image" type="fi:Image"/>
					<xs:element ref="dc:Description"/>
				</xs:choice>
			</xs:choice>
		</xs:sequence>
		<xs:attributeGroup ref="fi:statusEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="GRP" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeartypeOverview" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Gear type overview</Title>
				<Description>GeartypeOverview is a general description of a single fishing gear.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
			<xs:element name="FishTechniqueRef" type="fi:FishTechniqueRef"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeartypeRef" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Gear type reference</Title>
				<Description>is used for positive gear type identification of existing gear type objects. ISSSCFG or FIES code is required. Select a classification system, and enter the value in 'Code'.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:element name="ForeignID" type="fi:ForeignID"/>
			</xs:choice>
			<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6"/>
			<xs:element maxOccurs="unbounded" ref="dc:Identifier" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Geartypes" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Gear types</Title>
				<Description>Geartypes contains one or more instances of a general descriptions of fishing gears.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="GeartypeOverview" type="fi:GeartypeOverview"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GenEconReport" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>General Economic Report</Title>
				<Description>contains general economic reporting and economic data. Use fi:EconReport for reporting information and multiple instances of fi:GDPData to enter GDP data.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="GDPData" type="fi:GDPData"/>
			<xs:element name="EconReport" type="fi:EconReport"/>
			<xs:element name="Population" type="fi:Population"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeneralBiology" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>General Biology</Title>
				<Description>GeneralBiology is a generic description of the habitat and ecology of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Growth"/>
					<xs:enumeration value="Migratory"/>
					<xs:enumeration value="Shoaling"/>
					<xs:enumeration value="Behaviour"/>
					<xs:enumeration value="Preyed_upon_by"/>
					<xs:enumeration value="Ecology"/>
					<xs:enumeration value="Other_type"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="OtherType" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GenGeoData" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>General Geographic Data</Title>
				<Description>GenGeoData contains general geographic data about a country. Use the fi:Value attribute to enter the data value. Select the appropriate fi:Unit , and if applicable select the Area.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Area"/>
					<xs:enumeration value="Length"/>
					<xs:enumeration value="Shelf_area"/>
					<xs:enumeration value="Area_of_eez"/>
					<xs:enumeration value="Length_of_coastline"/>
					<xs:enumeration value="Area_of_lake"/>
					<xs:enumeration value="Length_of_river"/>
					<xs:enumeration value="Lagoon_area"/>
					<xs:enumeration value="Reservoir_area"/>
					<xs:enumeration value="Floodplain_area"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:areaUnit"/>
		<xs:attribute name="Area">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Marine"/>
					<xs:enumeration value="Inland"/>
					<xs:enumeration value="All"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Value" type="xs:string"/>
		<xs:attribute name="AdditionalInfo" type="xs:string"/>
		<xs:attribute name="Date" type="xs:date"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GenGeoEconReport" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>General Geographic and Economic Report</Title>
				<Description>GenGeoEconReport describes geographic and economic data at country level.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="GenGeoData" type="fi:GenGeoData"/>
			<xs:element name="GDPData" type="fi:GDPData"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GenGeoReport" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>General Geographic Report</Title>
				<Description>GenGeoReport contains country level geographic data. Use text for accompanying text , and fi:GenGeoData to enter actual data.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="GenGeoData" type="fi:GenGeoData"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Genus" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Genus</Title>
				<Description>GEN is genus.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" default="Taxonomic">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Taxonomic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GeoCoverage" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geographic Coverage</Title>
				<Description>is a container for information about an object's geographical coverage.</Description>
				<Usage>It should be used for describing the area on which a management body or authority exerts its mandate.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
			<xs:element name="MaritimeArea" type="fi:MaritimeArea"/>
			<xs:element name="InlandWaters" type="fi:InlandWaters"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="Type" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="RFB_CompetenceArea"/>
					<xs:enumeration value="AreaUnderNationalJurisdiction"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeoDist" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geographical distribution</Title>
				<Description>Information about the geographical distribution of an entity such as species, aquatic resource, ...</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="JurisdictionalDist" type="fi:JurisdictionalDist"/>
			<xs:group ref="fi:textEnt"/>
			<xs:element name="History" type="fi:History"/>
			<xs:element name="AddInfo" type="fi:AddInfo"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeoForm" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Bottom morphology</Title>
				<Description>"Marine habitat/Fishing ground: type of sea floor physiography in which the fishery resource is distributed or in which the fishery is conducted" (FIRMS Inventory Guidelines 2011). "Type of sea floor physiography in which the aquatic resource is distributed" (FIRMS TWG2, 2008). </Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Value" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Seamounts"/>
					<xs:enumeration value="Estuaries"/>
					<xs:enumeration value="Fjords"/>
					<xs:enumeration value="Canyons"/>
					<xs:enumeration value="Lagoons"/>
					<xs:enumeration value="Unspecified"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="FreeValue" use="optional"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GeoReference" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Geographic reference</Title>
				<Description>The geographic frame[work] from which fisheries are considered for inclusion in the inventory (modified from FIRMS, 2008).</Description>
				<Usage>"Fishery inventories are carried out at different scales (global, regional, national or sub-national), each time from a specific geographic standpoint. In a reporting system like FIRMS, this [scale x geographic] standpoint is herewith defined as the geo-reporting standpoint. Examples of geo-reporting standpoint are Western Central Pacific Ocean (an SPC  regional geo-reporting standpoint), Senegal (a national geo-reporting standpoint), or Florida (a USA sub-national geo-reporting standpoint). This geo-reporting standpoint defines the space within which fisheries are identified during an inventory" (FIRMS Information Management Policy).</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SpatialScale" type="fi:SpatialScale"/>
			<xs:choice minOccurs="0">
				<xs:element ref="dc:Title"/>
			</xs:choice>
			<xs:choice>
				<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
				<xs:element name="LandAreaRef" type="fi:LandAreaRef"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Glossary" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Glossary</Title>
				<Description>contains Fisheries definitions.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="GlossaryEntry" type="fi:GlossaryEntry"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="GlossaryEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Glossary entry</Title>
				<Description>contains a Fisheries definition.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="dc:Title"/>
			<xs:element ref="dc:Description"/>
			<xs:element name="Table" type="fi:Table"/>
			<xs:element ref="dc:Subject"/>
			<xs:element ref="dc:Relation"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Guidelines" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Guidelines</Title>
				<Description>contains information about guidelines related to legal systems such as environmental impact assessments.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="HabitatBio" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Habitat and biology</Title>
				<Description>Habitat and Biology is a container for information on the biology and ecology of a species in the area of the aquatic resource.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="ClimaticZone" type="fi:ClimaticZone"/>
			<xs:element name="DepthZone" type="fi:DepthZone"/>
			<xs:element name="DepthBehav" type="fi:DepthBehav"/>
			<xs:element name="BottomType" type="fi:BottomType"/>
			<xs:element name="HorizontalDist" type="fi:HorizontalDist"/>
			<xs:element name="VerticalDist" type="fi:VerticalDist"/>
			<xs:element name="GeoForm" type="fi:GeoForm"/>
			<xs:element name="History" type="fi:History"/>
			<xs:group ref="fi:textEnt"/>
			<!--following elements come from old dtd but still required-->
			<xs:element name="Reproduction" type="fi:Reproduction"/>
			<xs:element name="Feeding" type="fi:Feeding"/>
			<xs:element name="GeneralBiology" type="fi:GeneralBiology"/>
			<xs:element name="BiologicalParameters" type="fi:BiologicalParameters"/>
			<xs:element name="WaterCondition" type="fi:WaterCondition"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="HandlingMode" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Handling Mode</Title>
				<Description>HandlingMode is the container element containing description and information about the ways in which aquatic species are handled after being caught.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
			<!--<xs:element name="FishEquipRef" type="fi:FishEquipRef"/>-->
		</xs:choice>
		<xs:attribute name="Type" default="Various">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Freezer"/>
					<xs:enumeration value="Wetfish"/>
					<xs:enumeration value="Factory"/>
					<xs:enumeration value="Various"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="HandlingModeRef" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Handling Mode reference</Title>
				<Description>does not respond to any classification system. This element will be changed as a system is developed.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="6">
			<xs:element name="FigisID" type="fi:FigisID"/>
			<xs:element ref="dc:Title"/>
		</xs:choice>
		<!-- //// to be removed ////-->
	</xs:complexType>
	<xs:complexType name="HarvestedResourcesStatusTrend" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Status and trends of the Harvested Resources</Title>
				<Description>contains status and trend information about the harvested resources for a particular fishery.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:group ref="fi:textEnt" minOccurs="0" maxOccurs="unbounded"/>
		<xs:attribute name="FreeValue" use="optional"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="HarvestTechnique" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Diseases</Title>
				<Description>is a container for harvesting techniques for cultured species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Hatchery" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<dc:Title>Hatchery</dc:Title>
				<dc:Description>is a container for an instance of a hatchery.</dc:Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="Highlight" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Highlight</Title>
				<Description>is used to highlight.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Action"/>
					<xs:enumeration value="Activity"/>
					<xs:enumeration value="Costs"/>
					<xs:enumeration value="Constraint"/>
					<xs:enumeration value="Crew"/>
					<xs:enumeration value="Date"/>
					<xs:enumeration value="Days"/>
					<xs:enumeration value="Duration"/>
					<xs:enumeration value="GeartypeSelectivity"/>
					<xs:enumeration value="Issue"/>
					<xs:enumeration value="Mechanism"/>
					<xs:enumeration value="Method"/>
					<xs:enumeration value="Measure"/>
					<xs:enumeration value="Objective"/>
					<xs:enumeration value="Part"/>
					<xs:enumeration value="Peformance"/>
					<xs:enumeration value="Perspective"/>
					<xs:enumeration value="Recommend"/>
					<xs:enumeration value="RefPoint"/>
					<xs:enumeration value="Sector"/>
					<xs:enumeration value="Solution"/>
					<xs:enumeration value="Stakeholder"/>
					<xs:enumeration value="Status"/>
					<xs:enumeration value="StorMedium"/>
					<xs:enumeration value="Strategy"/>
					<xs:enumeration value="Trend"/>
					<xs:enumeration value="Uncertain"/>
					<xs:enumeration value="Unit"/>
					<xs:enumeration value="Value"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="History" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>History</Title>
				<Description>Historic information related to the element which contains fi:History. e.g.
an historical review of the Stock or Resource exploitation, management regimes over time and their effect, and trends.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="HoldCapacity" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Hold Capacity</Title>
				<Description>is a measurement of the capacity of a ship's hold, i.e. the capacity of the compartment below the deck.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="Unit" default="Cubic_metres"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="HorizontalDist" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Horizontal distribution</Title>
				<Description>"Marine habitat: inshore to offshore range in which the fishery resource is distributed (e.g. littoral, neritic, oceanic ...)." (FIRMS Inventory Guidelines 2011).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Value" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Inland_waters"/>
					<xs:enumeration value="Estuarine"/>
					<xs:enumeration value="Littoral"/>
					<xs:enumeration value="Neritic"/>
					<xs:enumeration value="Oceanic"/>
					<xs:enumeration value="Unspecified"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="FreeValue" use="optional"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Image" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Image</Title>
				<Description>Reference to an image, including its meta-data (title, image file name, legend, ...) formatting parameters. An image element may also includes a nested image element (e.g. link to high resolution version of the same image).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Image" type="fi:Image" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Title" type="xs:string" use="optional"/>
		<xs:attribute name="Description" type="xs:string"/>
		<xs:attribute name="Source" type="xs:string"/>
		<xs:attribute name="Use">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Normal"/>
					<xs:enumeration value="Extra"/>
					<xs:enumeration value="Thumb"/>
					<xs:enumeration value="Small"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Src" type="xs:string" use="optional"/>
		<xs:attribute name="IID" type="xs:string"/>
		<xs:attribute name="Link" type="xs:anyURI" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Images" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Images</Title>
				<Description>can hold text and/or other images for a gallery.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="IMOConfidence" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>LLoyd's ID Confidence Value</Title>
				<Description>contains confidence information for a Lloyd's ID that was obtained indirectly. Use dc:Title for each field compared and put the total number of fields compared in the NumberFieldsCompared attribute.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="dc:Title" maxOccurs="3"/>
		</xs:sequence>
		<xs:attribute name="NumberFieldsCompared" type="xs:positiveInteger"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ImpactEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Impact entry</Title>
				<Description>ImpactEntry is a descriptive container for text about the impacts of fishing on the environment , species and/or society.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="TargetSpecies" type="fi:TargetSpecies"/>
			<xs:element name="NotTargetSpecies" type="fi:NotTargetSpecies"/>
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
			<xs:element name="FishTechniqueRef" type="fi:FishTechniqueRef"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
		</xs:choice>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Environmental"/>
					<xs:enumeration value="Socio-economic"/>
					<xs:enumeration value="Species"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Impacts" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Impacts</Title>
				<Description>Impacts contains a list of impacts of fishing on the environment and/or society. Use one fi:ImpactEntry for each instance of an impact.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="ImpactEntry" type="fi:ImpactEntry"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Infrastructure" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Infrastructure</Title>
				<Description>contains infrastructure information for the container in which it is found.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:sectorEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Infringement" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Infringement</Title>
				<Description>is a container for information about a vessel's infringements.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="dc:Date"/>
			<xs:element name="DateReported" type="fi:DateReported"/>
			<xs:element name="DateObserved" type="fi:DateObserved"/>
			<xs:element name="MeasuresImposed" type="fi:MeasuresImposed"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="InlandProductionSector" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Inland Production Sector</Title>
				<Description>contains information on a country's inland subsectors.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="CaptureProduction" type="fi:CaptureProduction"/>
			<xs:element name="LandingSites" type="fi:LandingSites"/>
			<xs:element name="FishingSystems" type="fi:FishingSystems"/>
			<xs:element name="AqResOverview" type="fi:AqResOverview"/>
			<xs:element name="Management" type="fi:Management"/>
			<xs:element name="Community" type="fi:Community"/>
			<xs:element name="FisheryOverview" type="fi:FisheryOverview"/>
			<xs:element name="NASORef" type="fi:NASORef"/>
			<xs:element name="FACPRef" type="fi:FACPRef"/>
		</xs:choice>
		<xs:attribute name="Activity">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Industrial"/>
					<xs:enumeration value="Commercial"/>
					<xs:enumeration value="Artisanal"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="InlandWaters" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Inland Waters</Title>
				<Description>contains information about the type of inland waters.</Description>
				<Usage>Add this element within GeoCoverage to indicate if the Inland Waters are included or not by using the boolean Value attribute.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Value" type="xs:boolean" use="required"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Instance" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>RelatedStandards</Title>
				<Description>contains the description of the instance of a class.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Interactions" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Interactions with the Environment and Other Sectors</Title>
				<Description>contains information about a sector's Interactions with the Environment and Other Sectors.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="InterestFisheries" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Interest to Fisheries</Title>
				<Description>InterestFisheries contains information about the fisheries that exploit a species. General information on the fishery (Global catch , Gears , flag-states) can be inserted in the FISHERYText container , whereas main aspects of post-harvest fish utilisation should be put in fi:Utilisation. The impact that fisheries may have on the status of the species should be compiled in fi:Impacts.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="FishTechniqueOverview" type="fi:FishTechniqueOverview"/>
			<xs:element name="SocioEconReport" type="fi:SocioEconReport"/>
			<xs:element name="PostHarvest" type="fi:PostHarvest"/>
			<xs:element name="Measure" type="fi:Measure"/>
			<xs:element name="Impacts" type="fi:Impacts"/>
			<xs:element name="ConservStatus" type="fi:ConservStatus"/>
			<xs:element name="FishTechniqueRef" type="fi:FishTechniqueRef"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
		</xs:choice>
		<xs:attribute name="TypeFisheryExploitation" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Marine_mammals"/>
					<xs:enumeration value="Other"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Criteria">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[A|B|C|E|F|L|M|O|T][ther]?"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="International" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>International</Title>
				<Description>International is a container for information of type international.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="IntersectingAreas" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Intersecting Areas</Title>
				<Description>contains a system generated set of Water Area Reference blocks used for GIS mapping.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="IntLink" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Internal Link</Title>
				<Description>is used to make anchor points and links to anchor points within a document. Use the ID attribute to make an anchor, and IDREF to link to an anchor.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="Image" type="fi:Image"/>
		</xs:choice>
		<xs:attribute name="ID" type="xs:ID"/>
		<xs:attribute name="IDref" type="xs:IDREF"/>
	</xs:complexType>
	<xs:complexType name="IntroducedSpecies" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>IntroducedSpecies</Title>
				<Description>contains high-level information about IntroducedSpecies.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Introducer" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Introducer </Title>
				<Description>Who introduced the species</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Government"/>
					<xs:enumeration value="Individual"/>
					<xs:enumeration value="InternationalOrganization"/>
					<xs:enumeration value="PrivateSector"/>
					<xs:enumeration value="OtherIntroducer"/>
					<xs:enumeration value="Unknown"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="IntroSpecies" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Introduced Aquatic Species</Title>
				<Description>Is the Introduced Aquatic Species domain root element and it is used to create the OBJECT 'Introduced Aquatic Species'. Information related to this topic are taken from Dias 'Database on Introduction of Aquatic Species'(Firi). Important definitions for the purpose of this database are the following:Introduced Aquatic Species : An aquatic Species is called Introduced when it has been moved across a national border to a country outside its natural range (Welkomme, 1988; http://fao.org/fi/statist/fisoft/dias/glossary.htm).The element fi:IntroSpecies contains among the others three key elements: fi:IntroSpeciesIdent, fi:IntroSpeciesProfile and fi:IntroSpeciesFeatures. These are following the FiGis structure and logic to create the 'IntroducedAqSpecies' object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:sequence>
				<xs:element name="IntroSpeciesIdent" type="fi:IntroSpeciesIdent"/>
				<xs:element name="IntroSpeciesProfile" type="fi:IntroSpeciesProfile"/>
				<xs:element name="IntroSpeciesFeatures" type="fi:IntroSpeciesFeatures"/>
			</xs:sequence>
			<xs:group ref="fi:indexEnt" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="IntroSpeciesFeatures" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>IntroducedAqSpecies Features</Title>
				<Description>The Features the information are generally related to the effects of the IntroducedAqSpecies.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EstablishedWild" type="fi:EstablishedWild"/>
			<xs:element name="AquacultureUse" type="fi:AquacultureUse"/>
			<xs:element name="EcologicEffects" type="fi:EcologicEffects"/>
			<xs:element name="SocioEconomicEffects" type="fi:SocioEconomicEffects"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="IntroSpeciesIdent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Introduced Aquatic Species Identity</Title>
				<Description>Ident contains the elements needed to positively and uniquely Identify a domain object. Inclusion of this element in a document causes the FIGIS database loader to create a database object that will be keyed using the information found in the Ident element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:choice maxOccurs="unbounded">
					<xs:element name="SpeciesRef" type="fi:SpeciesRef"/>
					<xs:element name="Species" type="fi:Species"/>
					<xs:element name="SciName" type="fi:SciName"/>
				</xs:choice>
			</xs:choice>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="dc:Date"/>
				<xs:element name="Range" type="fi:Range"/>
			</xs:choice>
			<xs:element name="CountryTo" type="fi:CountryTo"/>
			<xs:element name="CountryFrom" type="fi:CountryFrom" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="IntroSpeciesProfile" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>IntroducedAqSpecies Profile </Title>
				<Description>A profile of the introduced species</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
			</xs:choice>
			<xs:element name="ReasonIntroducedAqSpecies" type="fi:ReasonIntroducedAqSpecies"/>
			<xs:element name="Introducer" type="fi:Introducer"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ISSCAAPCode" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>ISSCAAP Aquatic Species Group Code</Title>
				<Description>ISSCAAPCode is the International Standard Statistical Classification of Aquatic Animals and Plants. Equivalence - this attribute gives a value to the relation between a custom group and the code system used to define the custom group. This relation is defined as follows: equivalent : the code used is equivalent to the custom group.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" default="Taxonomic">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Taxonomic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="Equivalence">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Equivalent"/>
					<xs:enumeration value="Standard_is_broader"/>
					<xs:enumeration value="Standard_is_narrower"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ISSCFGCode" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>ISSCFG Code</Title>
				<Description>ISSCFGCode is the standard classification system for gear types.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Num"/>
					<xs:enumeration value="3-alpha"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ISSCFVCode" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>ISSCFV Code</Title>
				<Description>ISSCFVCode is one of the standard classification systems for vessel types.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Num"/>
					<xs:enumeration value="3-alpha"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="IssueEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Issue</Title>
				<Description>is a container for an instance of a fisheries issue.</Description>
			</xs:documentation>
		</xs:annotation>
