			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Projection" type="fi:Projection"/>
			<xs:element name="Performance" type="fi:Performance"/>
			<xs:element name="Management" type="fi:Management"/>
