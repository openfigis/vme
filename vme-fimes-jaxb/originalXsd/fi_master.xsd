			<xs:element name="Management" type="fi:Management"/>
			<xs:element name="Markets" type="fi:Markets"/>
			<xs:element name="Performance" type="fi:Performance"/>
			<xs:element name="StatusTrends" type="fi:StatusTrends"/>
			<xs:element name="Development" type="fi:Development"/>
