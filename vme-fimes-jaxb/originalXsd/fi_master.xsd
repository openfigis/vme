<?xml version="1.0" encoding="UTF-8"?>
<!-- edited  by Yves Jaques, Aureliano gentile (FAO Headquarters) -->
<?xml-stylesheet type="text/xsl" href="global_elems.xslt"?>
<!-- global_schema_to_domain.xslt -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:ags="http://www.purl.org/agmes/1.1/" xmlns:aida="http://www.idmlinitiative.org/resources/dtds/AIDA22.xsd" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:dwc="http://rs.tdwg.org/dwc/terms/" xmlns:fi="http://www.fao.org/fi/figis/devcon/" targetNamespace="http://www.fao.org/fi/figis/devcon/" elementFormDefault="qualified">
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
	<xs:import namespace="http://purl.org/dc/elements/1.1/" schemaLocation="dc.xsd"/>
	<xs:import namespace="http://rs.tdwg.org/dwc/terms/" schemaLocation="dwc.xsd"/>
	<xs:import namespace="http://purl.org/dc/terms/" schemaLocation="dcterms.xsd"/>
	<xs:import namespace="http://www.purl.org/agmes/1.1/" schemaLocation="ags.xsd"/>
	<xs:import namespace="http://www.idmlinitiative.org/resources/dtds/AIDA22.xsd" schemaLocation="aida.xsd"/>
	<xs:annotation>
		<xs:documentation>
			<Title>Fisheries Metadata Element Set (FIMES)</Title>
			<Description>To manage information in a systematic and cost-effective way, FIGIS has built a metadata level (a topic tree) for each information domain, providing a standard template for the display of Web page content. The Fisheries Metadata Element Set (FIMES) describes this meta level which is based on XML language.</Description>
		</xs:documentation>
	</xs:annotation>
	<xs:element name="FIGISDoc" type="fi:FIGISDoc"/>
	<xs:complexType name="FIGISDoc" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>figis Document</Title>
				<Description>the root element and root DTD for all FIGIS objects. Select FIGISDoc to begin a new XML document and then complete the required DataEntry and DocSource elements before selecting the desired domain in which to begin creating a new resource.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DataEntry" type="fi:DataEntry"/>
			<xs:element name="ObjectSource" type="fi:ObjectSource"/>
			<xs:choice>
				<xs:element name="AqRes" type="fi:AqRes"/>
				<xs:element name="Collection" type="fi:Collection"/>
				<xs:element name="CountrySector" type="fi:CountrySector"/>
				<xs:element name="CultureSpecies" type="fi:CultureSpecies"/>
				<xs:element name="EAFTool" type="fi:EAFTool"/>
				<xs:element name="FishEquip" type="fi:FishEquip"/>
				<xs:element name="Fishery" type="fi:Fishery"/>
				<xs:element name="FishTechnique" type="fi:FishTechnique"/>
				<xs:element name="Geartype" type="fi:Geartype"/>
				<xs:element name="LegalFramework" type="fi:LegalFramework"/>
				<xs:element name="ManageSystem" type="fi:ManageSystem"/>
				<xs:element name="PortStateMeasure" type="fi:PortStateMeasure"/>
				<xs:element name="Standard" type="fi:Standard"/>
				<xs:element name="Org" type="fi:Org"/>
				<xs:element name="Species" type="fi:Species"/>
				<xs:element name="Topic" type="fi:Topic"/>
				<xs:element name="VesselRecord" type="fi:VesselRecord"/>
				<xs:element name="Vesseltype" type="fi:Vesseltype"/>
				<xs:element name="VME" type="fi:VME"/>
			</xs:choice>
			<xs:element name="Tags" type="fi:Tags" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:group name="indexEnt">
		<xs:choice>
			<xs:element name="Sources" type="fi:Sources"/>
			<xs:element name="Bibliography" type="fi:Bibliography"/>
			<xs:element name="RelatedResources" type="fi:RelatedResources"/>
		</xs:choice>
	</xs:group>
	<xs:group name="textEnt">
		<xs:choice>
			<xs:element name="Text" type="fi:Text"/>
			<xs:element name="Image" type="fi:Image"/>
			<xs:element name="Table" type="fi:Table"/>
			<xs:element name="Chart" type="fi:Chart"/>
			<xs:element name="KnowledgeObjRef" type="fi:KnowledgeObjRef"/>
			<xs:element name="Contacts" type="fi:Contacts"/>
			<xs:element name="FIGISSearch" type="fi:FIGISSearch"/>
			<xs:element name="FIGISCondLink" type="fi:FIGISCondLink"/>
		</xs:choice>
	</xs:group>
	<xs:group name="textEntNoKOR">
		<xs:choice>
			<xs:element name="Text" type="fi:Text"/>
			<xs:element name="Image" type="fi:Image"/>
			<xs:element name="Table" type="fi:Table"/>
			<xs:element name="Chart" type="fi:Chart"/>
		</xs:choice>
	</xs:group>
	<xs:group name="textEntNoTitle">
		<xs:choice>
			<xs:element name="Text" type="fi:Text"/>
			<xs:element name="Image" type="fi:Image"/>
			<xs:element name="Table" type="fi:Table"/>
		</xs:choice>
	</xs:group>
	<xs:group name="textEntMandTitle">
		<xs:sequence>
			<xs:element ref="dc:Title"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Text" type="fi:Text"/>
				<xs:element name="Image" type="fi:Image"/>
				<xs:element name="Table" type="fi:Table"/>
			</xs:choice>
		</xs:sequence>
	</xs:group>
	<xs:attributeGroup name="ageType">
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="First_maturity"/>
					<xs:enumeration value="Maximum"/>
					<xs:enumeration value="Infinity"/>
					<xs:enumeration value="Other_type"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="areaUnit">
		<xs:attribute name="Unit" default="sq_km">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="km"/>
					<xs:enumeration value="sq_km"/>
					<xs:enumeration value="mile"/>
					<xs:enumeration value="sq_mile"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="byType">
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="By_species"/>
					<xs:enumeration value="By_area"/>
					<xs:enumeration value="By_environment"/>
					<xs:enumeration value="By_culture_system"/>
					<xs:enumeration value="By_farming_system"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="countryEnt">
		<xs:attribute name="country">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[A-Z]{3,}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="environEnt">
		<xs:attribute name="Environ">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="All"/>
					<xs:enumeration value="Marine"/>
					<xs:enumeration value="Brackish"/>
					<xs:enumeration value="Freshwater"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="legalStatusEnt">
		<xs:attribute name="LegalStatus">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Enforced"/>
					<xs:enumeration value="Signed"/>
					<xs:enumeration value="Working_proposal"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="lengthUnit">
		<xs:attribute name="Unit" default="metres">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="km"/>
					<xs:enumeration value="metres"/>
					<xs:enumeration value="cm"/>
					<xs:enumeration value="inches"/>
					<xs:enumeration value="feet"/>
					<xs:enumeration value="yards"/>
					<xs:enumeration value="miles"/>
					<xs:enumeration value="fathoms"/>
					<xs:enumeration value="leagues"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="monetaryScale">
		<xs:attribute name="Currency">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[A-Z]{3,}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Scale">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="One"/>
					<xs:enumeration value="Ten"/>
					<xs:enumeration value="Hundred"/>
					<xs:enumeration value="Thousand"/>
					<xs:enumeration value="Million"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="sectorEnt">
		<xs:attribute name="Sector">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Fishery_and_aquaculture"/>
					<xs:enumeration value="Aquaculture"/>
					<!--					
					<xs:enumeration value="Fisheries"/>
					<xs:enumeration value="Marine_capture"/>					
					<xs:enumeration value="Inland_capture"/>-->
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="speciesAtt">
		<xs:attribute name="OtherType" type="xs:string"/>
		<xs:attribute name="OtherUnit" type="xs:string"/>
		<xs:attribute name="Land" type="xs:string"/>
		<xs:attribute name="Time" type="xs:string"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="statusEnt">
		<xs:attribute name="Status" type="xs:integer" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="volumeUnit">
		<xs:attribute name="Unit" default="litre">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="cu_m"/>
					<xs:enumeration value="cu_ft"/>
					<xs:enumeration value="litre"/>
					<xs:enumeration value="cc"/>
					<xs:enumeration value="gallon"/>
					<xs:enumeration value="oz"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="weightUnit">
		<xs:attribute name="Unit" default="kg">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="kg"/>
					<xs:enumeration value="g"/>
					<xs:enumeration value="lb"/>
					<xs:enumeration value="piece"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="code">
		<xs:attribute name="Code" type="xs:string" use="required"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="asfis">
		<xs:attribute name="CodeSystemType" default="ASFIS">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="ASFIS"/>
					<xs:enumeration value="SIDP"/>
					<xs:enumeration value="FishFinder"/>
					<xs:enumeration value="Eschmeyer"/>
					<xs:enumeration value="Other"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:complexType name="AbundanceLevel" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Abundance Level</Title>
				<Description>"Degree of plentifulness. The total number of fish in a population or on a fishing ground. Can be measured in absolute or relative terms." (FAO Fisheries Glossary)</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="StandardAbundanceLevel" type="fi:StandardAbundanceLevel"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="Type" use="optional" default="Unspecified">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="CCAMLR"/>
					<xs:enumeration value="CCSBT"/>
					<xs:enumeration value="CECAF"/>
					<xs:enumeration value="EUROSTAT"/>
					<xs:enumeration value="FAO"/>
					<xs:enumeration value="FIRMS"/>
					<xs:enumeration value="GFCM"/>
					<xs:enumeration value="IATTC"/>
					<xs:enumeration value="ICCAT"/>
					<xs:enumeration value="ICES"/>
					<xs:enumeration value="IOTC"/>
					<xs:enumeration value="NAFO"/>
					<xs:enumeration value="NEAFC"/>
					<xs:enumeration value="SEAFDEC"/>
					<xs:enumeration value="SEAFO"/>
					<xs:enumeration value="WECAFC"/>
					<xs:enumeration value="Unspecified"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="FreeValue" use="optional"/>
	</xs:complexType>
	<xs:complexType name="Access" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>SiteIdentification</Title>
				<Description>contains information about procedures involved in identifying a suitable site in which to engage in aquaculture.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Lease" type="fi:Lease"/>
			<xs:element name="Legislation" type="fi:Legislation"/>
			<xs:element name="OrgsInvolved" type="fi:OrgsInvolved"/>
			<xs:element name="TermsAndConditions" type="fi:TermsAndConditions"/>
			<xs:element name="Procedures" type="fi:Procedures"/>
			<xs:element name="ManageSystemOverview" type="fi:ManageSystemOverview"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AccuracyIndicator" mixed="false">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Acknowledgement" mixed="false">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Acronym" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Acronym</Title>
				<Description>contains the abbreviated name of an object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Action" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Action</Title>
				<Description>contains information about Fisheries actions.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ActionPlan" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Plan of Action</Title>
				<Description>means a management system plan of action applied to a vesseltype, geartype, fishery, fishing technique, stock, resource, managment unit, species or species group.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="International" type="fi:International"/>
			<xs:element name="Regional" type="fi:Regional"/>
			<xs:element name="Local" type="fi:Local"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<!-- <xs:complexType name="Activities" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>FI Sector Activities</Title>
				<Description>Activities contains country level fisheries development activities information. Use it when fi:Research , fi:SubsectorOverallProjects or fi:AidOrInvestments are not appropriate.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType> -->
	<xs:complexType name="AddInfo" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Additional Information</Title>
				<Description>contains additional information related to an object that is deemed of secondary importance.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Factsheet" type="fi:Factsheet"/>
		</xs:choice>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="General_information"/>
					<xs:enumeration value="Additional_information"/>
					<xs:enumeration value="Note"/>
					<xs:enumeration value="Remarks"/>
					<xs:enumeration value="Footnote"/>
					<xs:enumeration value="Endnote"/>
					<xs:enumeration value="Popup"/>
					<xs:enumeration value="Expand"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Address" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>PostalAddress Line 1</Title>
				<Description> fi:AddrLine1 contains the first line of the postal fi:PostalAddress of a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="LineNumber" type="xs:string" use="required"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AdditionalRefData" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Additional Reference Data</Title>
				<Description>contains data found in a Ref block that is not part of the RTMS.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="dcterms:Alternative"/>
		</xs:choice>
		<xs:attribute name="Style">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Full"/>
					<xs:enumeration value="NoImage"/>
					<xs:enumeration value="Link"/>
					<xs:enumeration value="Icon"/>
					<xs:enumeration value="Expand"/>
					<xs:enumeration value="More"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Layout">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Block"/>
					<xs:enumeration value="Bullet"/>
					<xs:enumeration value="Indent"/>
					<xs:enumeration value="BulletAndIndent"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Advice" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Management advice</Title>
				<Description>"Management advice are those recommendations generally evolving from the assessment addressed to management authorities. It evolves from biological scientific advice (found under assessment results) but takes into account a broader spectrum of issues. Advice may be proposed under different management options, each one including a risk assessment element" (FIGIS-FIRMS 2002 workshop).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Age" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Age</Title>
				<Description>Age is a container for information directly related to the Age of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="Min" type="fi:Min"/>
			<xs:element name="Max" type="fi:Max"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attributeGroup ref="fi:ageType"/>
		<xs:attribute name="Unit" default="Year">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Year"/>
					<xs:enumeration value="Other_unit"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:speciesAtt"/>
	</xs:complexType>
	<xs:complexType name="Aggregation" mixed="false">
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Aggregation" type="fi:Aggregation"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AidOrInvestments" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aid or Investments</Title>
				<Description>On-going or planned projects, with their objectives, time scales, institutions involved, expected outcomes for the fishery and aquaculture sectors. For management systems, investments and subsidies in fisheries with reference to management systems to which these investments/subsidies apply.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="OrgOverview" type="fi:OrgOverview"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Application" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Application Profile</Title>
				<Description>allows the file to be displayed differently by the formatting engine. For example an ORG file could be labelled as profile RFB, and will then be formatted as such.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Profile">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="RFB"/>
					<xs:enumeration value="IPOA"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ThematicApproach" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Thematic Approach</Title>
				<Description>"The thematic approach highlights the disciplinary viewpoint prevailing in the
identification of fishery units" (FIRMS TWG2, 2008).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Fishery Resource"/>
					<xs:enumeration value="Jurisdictional"/>
					<xs:enumeration value="Production System"/>
					<xs:enumeration value="Fishery Management Unit"/>
					<xs:enumeration value="Fishing Activity"/>
					<xs:enumeration value="Access Rights"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="ApproachEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Approach</Title>
				<Description>is a single instance of an approach used by a management system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Title"/>
				<xs:group ref="fi:textEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Approaches" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Approaches</Title>
				<Description>are the approaches used by a management system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="ApproachEntry" type="fi:ApproachEntry"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AqRes" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic resource</Title>
				<Description>According to the FAO Fisheries Glossary, "Biotic element of the aquatic ecosystem, including genetic resources, organisms or parts thereof, populations, etc. with actual or potential use or value (sensu lato) for humanity. Fishery resources are those aquatic resources of value to fisheries".</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AqResIdent" type="fi:AqResIdent"/>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="History" type="fi:History"/>
				<xs:element name="HabitatBio" type="fi:HabitatBio"/>
				<xs:element name="GeoDist" type="fi:GeoDist"/>
				<xs:element name="WaterAreaOverview" type="fi:WaterAreaOverview"/>
				<xs:element name="AqResStruct" type="fi:AqResStruct"/>
				<xs:element name="Exploit" type="fi:Exploit"/>
				<xs:element name="Statistics" type="fi:Statistics"/>
				<xs:element name="BioAssess" type="fi:BioAssess"/>
				<xs:element name="Management" type="fi:Management"/>
				<xs:element name="AqResStateTrend" type="fi:AqResStateTrend"/>
				<xs:element name="RelatedObs" type="fi:RelatedObs"/>
				<xs:group ref="fi:indexEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AqResIdent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic resource identity</Title>
				<Description>Topic element used to specify the identity of the aquatic resource. An aquatic resource is identified by a title, an association of references to Species and Water Areas, or a unique FIGIS system identifier. </Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:element ref="dc:Title"/>
				<xs:sequence>
					<xs:element name="SpeciesList" type="fi:SpeciesList" maxOccurs="unbounded"/>
					<xs:element name="WaterAreaList" type="fi:WaterAreaList"/>
					<xs:element name="ReferenceYear" type="fi:ReferenceYear"/>
				</xs:sequence>
			</xs:choice>
			<xs:element name="ReportingYear" type="fi:ReportingYear" minOccurs="0"/>
			<xs:element name="ForeignID" type="fi:ForeignID" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AdditionalRefData" type="fi:AdditionalRefData" minOccurs="0"/>
			<xs:element name="Monitoring" type="fi:Monitoring" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="fi:statusEnt"/>
		<xs:attribute name="Rank" type="xs:positiveInteger"/>
		<xs:attribute name="Factsheet" type="xs:boolean"/>
		<xs:attribute name="RefObservation" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="AqResOverview" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic Resource overview</Title>
				<Description>Aquatic Resource overview contains high-level background information on the Aquatic resource instances.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="AqResRef" type="fi:AqResRef"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AqResRef" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic resource reference</Title>
				<Description> Element used to Refer to an aquatic resource. It contains species and water area domain elements which together enable positive referencing of an aquatic resource.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:element ref="dc:Title"/>
				<xs:element name="ForeignID" type="fi:ForeignID"/>
			</xs:choice>
			<xs:element ref="dcterms:Alternative" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="SpeciesRef" type="fi:SpeciesRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="WaterAreaRef" type="fi:WaterAreaRef" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="CollectionRef" type="fi:CollectionRef" minOccurs="0"/>
			<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AqResStateTrendEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic Resource State and Trend information entry</Title>
				<Description>Use an fi:AqResStateTrendEntry for each instance of a state or trend statement, in case distinct statements applying to different aquatic resources under review are included.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element name="ExploitRate" type="fi:ExploitRate"/>
				<xs:element name="AbundanceLevel" type="fi:AbundanceLevel"/>
			</xs:sequence>
			<xs:element name="ExploitState" type="fi:ExploitState" minOccurs="0"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Title"/>
				<xs:group ref="fi:textEnt"/>
				<xs:element name="AqResRef" type="fi:AqResRef"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AqResStateTrend" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquatic Resource State and Trends</Title>
				<Description>AqResStateTrend is a container for Aquatic Resource State and Trend information entries. "This element includes a short statement (which should be understandable by the public) summarising the current observation made on the resource or stock considered, with a focus on the state of the fish population and the trends characterising its evolution, and possibly outlooks on its future. For resources units defined at aggregated level, this topic may consist of a general statement including a list of state and trends entries fitting each of the sub-units making up that aggregated resource" (FIGIS-FIRMS 2002 workshop).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element name="ExploitRate" type="fi:ExploitRate"/>
				<xs:element name="AbundanceLevel" type="fi:AbundanceLevel"/>
			</xs:sequence>
			<xs:element name="ExploitState" type="fi:ExploitState" minOccurs="0"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="dc:Title"/>
				<xs:element name="AqResStateTrendEntry" type="fi:AqResStateTrendEntry"/>
				<xs:group ref="fi:textEnt"/>
				<xs:element name="History" type="fi:History"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AqResStruct" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Resource structure</Title>
				<Description>Information about structure of the aquatic resource. This element includes an attribute describing whether Yes or Not the aquatic resource is considered a biological Stock. It might as well introduce one or more subunit(s) of the aquatic resource under consideration (use fi:AqStockRef to positively refer a lower level of aggregation of an aquatic resource). Where possible, a short explanation should be provided to justify the existence of the sub-unit(s).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="AqResRef" type="fi:AqResRef"/>
			<xs:element name="History" type="fi:History"/>
		</xs:choice>
		<xs:attribute name="BiologicalStock" type="xs:boolean" use="required"/>
	</xs:complexType>
	<xs:complexType name="AquacultureProduction" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquaculture Production</Title>
				<Description>contains information about aquaculture production.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="FisheryIndicator" type="fi:FisheryIndicator"/>
			<xs:element name="FisheryIndicatorList" type="fi:FisheryIndicatorList"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AquacultureSector" mixed="false">
		<!-- this is equivalent to AquacultureSectorIndustryProfile it will be deleted when NASO will be migrated to the new version -->
		<xs:annotation>
			<xs:documentation>
				<Title>Aquaculture SubSector</Title>
				<Description>contains information on a Sector's aquaculture sector. Select the environment Type.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="AquacultureProduction" type="fi:AquacultureProduction"/>
			<xs:element name="CultureSystems" type="fi:CultureSystems"/>
			<xs:element name="FarmingSystems" type="fi:FarmingSystems"/>
			<xs:element name="ProductionSites" type="fi:ProductionSites"/>
			<xs:element name="CulturedSpecies" type="fi:CulturedSpecies"/>
			<xs:element name="Credit" type="fi:Credit"/>
			<xs:element name="Interactions" type="fi:Interactions"/>
			<xs:element name="Employment" type="fi:Employment"/>
			<xs:element name="Stakeholders" type="fi:Stakeholders"/>
			<xs:element name="Community" type="fi:Community"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:environEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<!-- this is equivalent to AquacultureSectorIndustryProfile it will be deleted when NASO will be migrated to the new version -->
	</xs:complexType>
	<xs:complexType name="AquacultureProductionSector" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquaculture Production Sector</Title>
				<Description>contains information on a Sector's aquaculture sector. Select the environment Type.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="AquacultureProduction" type="fi:AquacultureProduction"/>
			<xs:element name="CultureSystems" type="fi:CultureSystems"/>
			<xs:element name="FarmingSystems" type="fi:FarmingSystems"/>
			<xs:element name="ProductionSites" type="fi:ProductionSites"/>
			<xs:element name="CulturedSpecies" type="fi:CulturedSpecies"/>
			<xs:element name="Credit" type="fi:Credit"/>
			<xs:element name="Interactions" type="fi:Interactions"/>
			<xs:element name="Employment" type="fi:Employment"/>
			<xs:element name="Stakeholders" type="fi:Stakeholders"/>
			<xs:element name="Community" type="fi:Community"/>
			<xs:element name="NASORef" type="fi:NASORef"/>
			<xs:element name="FACPRef" type="fi:FACPRef"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:environEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AquacultureUse" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Aquaculture Use</Title>
				<Description>Element contains information on weather the speceis is used in Aquaculture and if this is the case which is the most use way of reproduce it.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Widely_used"/>
					<xs:enumeration value="Rarely_used"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="ReproductionMode">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Assisted_artificial"/>
					<xs:enumeration value="Natural"/>
					<xs:enumeration value="Continuous_imports"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Area" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>figis Area</Title>
				<Description>Area is the root element of domain Area, select the Ref container for positive linking and detailed description, and select Ident only when the goal is to create a new database object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:sequence>
				<xs:choice>
					<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
					<xs:element name="LandAreaRef" type="fi:LandAreaRef"/>
				</xs:choice>
				<xs:choice maxOccurs="unbounded">
					<xs:element name="AreaProfile" type="fi:AreaProfile"/>
					<xs:element name="AreaFeature" type="fi:AreaFeature"/>
				</xs:choice>
			</xs:sequence>
			<xs:sequence>
				<xs:element name="AreaIdent" type="fi:AreaIdent"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="AreaProfile" type="fi:AreaProfile"/>
					<xs:element name="AreaFeature" type="fi:AreaFeature"/>
				</xs:choice>
				<xs:group ref="fi:indexEnt" minOccurs="0"/>
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Area"/>
					<xs:enumeration value="Subarea"/>
					<xs:enumeration value="Division"/>
					<xs:enumeration value="Subdivision"/>
					<xs:enumeration value="Unit"/>
					<xs:enumeration value="Subunit"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AreaFeature" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Area Feature</Title>
				<Description>AreaFeature can contain any domain which has a geo-political aspect to its definition.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AreaIdent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>figis Area Identity</Title>
				<Description>AreaIdent contains all the possible codes used to positively identify an area object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="dc:Title" maxOccurs="6"/>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:choice>
					<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
					<xs:element name="LandAreaRef" type="fi:LandAreaRef"/>
				</xs:choice>
			</xs:choice>
			<xs:element name="Image" type="fi:Image" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Water"/>
					<xs:enumeration value="Land"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:statusEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AreaProfile" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Area Profile</Title>
				<Description>AreaProfile contains the elements used by domain specialists to differentiate objects from one another.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="AreaStruct" type="fi:AreaStruct"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AreaStruct" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Area Structure</Title>
				<Description>AreaStruct contains sub areas of the fi:Area container in which it's found. Add another fi:Area for every subarea and they will be created as new objects in the FIGIS database.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
			<xs:element name="LandAreaRef" type="fi:LandAreaRef"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ArrangementEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Arrangemententry</Title>
				<Description>contains information about an instance of an arrangement.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="LegalRelation" type="fi:LegalRelation"/>
			<xs:element name="OrgsInvolved" type="fi:OrgsInvolved"/>
			<xs:element name="SpatialScale" type="fi:SpatialScale"/>
		</xs:choice>
		<xs:attribute name="Role" type="xs:string"/>
		<xs:attributeGroup ref="fi:legalStatusEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Arrangements" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Arrangements</Title>
				<Description>contains information about legal arrangements.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="ArrangementEntry" type="fi:ArrangementEntry"/>
			<xs:element name="OrgsInvolved" type="fi:OrgsInvolved"/>
			<xs:element name="SpatialScale" type="fi:SpatialScale"/>
		</xs:choice>
		<xs:attribute name="Role" type="xs:string"/>
		<xs:attributeGroup ref="fi:legalStatusEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AssessData" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment data</Title>
				<Description>Data contains data, or information about data, used in the context of this assessment method to generate the exploitation indicators, or presented to the scientific assessment for the calculation of assessment indicators.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="Value" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Fishery Catch and Effort"/>
					<xs:enumeration value="Vessel Surveys"/>
					<xs:enumeration value="Tagging"/>
					<xs:enumeration value="Remote Sensing"/>
					<xs:enumeration value="Environmental Data"/>
					<xs:enumeration value="Others"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AssessIndicator" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment indicator</Title>
				<Description>Assessment indicator contains indicator data commonly generated using stock assessment methods (e.g. fishing mortality, biomass, recruitment, abundance index). According to the FAO Fisheries Glossary, an indicator is "a variable, pointer, or index. Its fluctuation reveals the variations in key elements of a system. The position and trend of the indicator in relation to reference points or values indicate the present state and dynamics of the system. Indicators provide a bridge between objectives and action".</Description>
			</xs:documentation>
			<xs:appinfo>
				<restrict>
					<restrictDomain>AqRes</restrictDomain>
				</restrict>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="Value" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Biomass"/>
					<xs:enumeration value="Abundance"/>
					<xs:enumeration value="Exploitation rate"/>
					<xs:enumeration value="SSB"/>
					<xs:enumeration value="Recruitment"/>
					<xs:enumeration value="Fishing mortality"/>
					<xs:enumeration value="Average Weight"/>
					<xs:enumeration value="Average length"/>
					<xs:enumeration value="Others"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AssessModels" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment models</Title>
				<Description>Assessment models describes the various models used in performing a stock assessment. It might contain a reference list of models commonly used in stock assessments.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Assumption" type="fi:Assumption"/>
			<xs:element name="Methodology" type="fi:Methodology"/>
			<xs:element name="AssessModelEntry" type="fi:AssessModelEntry"/>
		</xs:choice>
		<xs:attribute name="Value" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Age-structured"/>
					<xs:enumeration value="Size-structured"/>
					<xs:enumeration value="Biomass-aggregated"/>
					<xs:enumeration value="Others"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AssessModelEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment model entry</Title>
				<Description>Use fi:AssessModelEntry for each model used in performing a stock assessment. "An assessment model is identified by its title, and ideally includes the description of available data, assumptions made and methodology applied, and a summary of results".</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEntMandTitle"/>
			</xs:choice>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Assumption" type="fi:Assumption"/>
				<xs:element name="Methodology" type="fi:Methodology"/>
				<xs:element name="Data" type="fi:Data"/>
				<xs:element name="Results" type="fi:Results"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="Type" type="xs:string"/>
	</xs:complexType>
	<!-- /////////////// emprical assessment ////////////////// -->
	<xs:complexType name="AssessEmpiricalApproach" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Empirical Assessments</Title>
				<Description>Assessments based on an estimation or observation (e.g. catch only, catch trends, delphi approaches) and not based on analytical assessment models.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="AssessEmpiricalApproachEntry" type="fi:AssessEmpiricalApproachEntry"/>
		</xs:choice>
		<xs:attribute name="Value" type="xs:string" use="optional"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AssessEmpiricalApproachEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Empirical Assessment entry</Title>
				<Description>Use fi:AssessEmpiricalApproachEntry for each empirical approach used in performing a resource assessment, e.g. “catch and cpue trends”, “catch only”, “Delphi method”, etc.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEntMandTitle"/>
			</xs:choice>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Results" type="fi:Results"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AssessmentSummary" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment summary</Title>
				<Description>xxxxx</Description>
			</xs:documentation>
			<xs:appinfo>
				<restrict>
					<restrictDomain>Fishery</restrictDomain>
				</restrict>
			</xs:appinfo>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="RelatedAquaticResources" type="fi:RelatedAquaticResources"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Assumption" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Assumption</Title>
				<Description>Assumption is a description of the underlying assumptions upon which stock assessments, or results are based.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AuthorisationSystem" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>AuthorisationSystem</Title>
				<Description>contains information about legal authorisation systems for aquaculture and/or fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="License" type="fi:License"/>
			<xs:element name="Concession" type="fi:Concession"/>
			<xs:element name="OrgsInvolved" type="fi:OrgsInvolved"/>
			<xs:element name="TermsAndConditions" type="fi:TermsAndConditions"/>
			<xs:element name="Procedures" type="fi:Procedures"/>
			<xs:element name="SiteIdentification" type="fi:SiteIdentification"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Avg" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Average</Title>
				<Description>contains a numeric average value.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Background" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Background</Title>
				<Description>contains Background information on Fisheries Topics.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Bathymetry" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Bathymetry</Title>
				<Description>Bathymetry is numeric depth information about the ranges in which an aquatic species is found.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="Min" type="fi:Min"/>
			<xs:element name="Max" type="fi:Max"/>
			<xs:element name="DiscreteValue" type="fi:DiscreteValue"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:lengthUnit"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Beam" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Beam</Title>
				<Description>is the width of a vessel at its widest point.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:lengthUnit"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BiblioEntry" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Bibliographic entry</Title>
				<Description>BiblioEntry is a container for elements that define a reference source. It is similar in spirit to the bibliographies found in traditional books. All the fields that would be expected in a Bibliographic Entry are included, as well as the possibility of tagging hyperlinks and adding free text.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="dc:Title"/>
			<xs:element name="Series" type="fi:Series"/>
			<xs:element ref="ags:CreatorPersonal"/>
			<xs:element ref="ags:CreatorCorporate"/>
			<xs:element name="Editor" type="fi:Editor"/>
			<xs:element ref="dcterms:Created"/>
			<xs:element ref="dcterms:Modified"/>
			<xs:element ref="dc:Identifier"/>
			<xs:element ref="dc:Description"/>
			<xs:element ref="dcterms:Abstract"/>
			<xs:element ref="dc:Subject"/>
			<xs:element ref="dc:Publisher"/>
			<xs:element ref="dc:Language"/>
			<xs:element ref="dcterms:Extent"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Bibliography" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Bibliography</Title>
				<Description>Bibliography is a container for biblio entry elements. It is used as in a traditional print bibliography found at the end of a document, in order to cite the literature consulted and used for the creation of the document.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="BiblioEntry" type="fi:BiblioEntry"/>
		</xs:choice>
		<xs:attribute name="RefersTo">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Block"/>
					<xs:enumeration value="Entire_file"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BioAssess" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Assessment</Title>
				<Description>Assessment is a container for elements describing the biological assessment of the aquatic resource, such as the "description of data sources used for the assessment, the various assessment models and related methods applied with their individual results, and the overall assessment results, including possibly scientific advice" (FIGIS-FIRMS 2002 workshop). </Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="AssessModels" type="fi:AssessModels"/>
			<xs:element name="AssessEmpiricalApproach" type="fi:AssessEmpiricalApproach"/>
			<xs:element name="Data" type="fi:Data"/>
			<xs:element name="Results" type="fi:Results"/>
			<xs:element name="SciAdvice" type="fi:SciAdvice"/>
			<xs:element name="History" type="fi:History"/>
			<xs:element name="Uncertainty" type="fi:Uncertainty"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BiologicalControl" mixed="false">
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="SnailControl"/>
					<xs:enumeration value="WeedControl"/>
					<xs:enumeration value="OtherPestControl"/>
					<xs:enumeration value="MosquitoControl"/>
					<xs:enumeration value="PlanktonControl"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BiologicalParameters" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Biological Parameters</Title>
				<Description>Biological Parameters is a container for information directly related to the parameters used to describe the biology of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
			</xs:choice>
			<xs:element name="Length" type="fi:Length" minOccurs="0"/>
			<xs:element name="Weight" type="fi:Weight" minOccurs="0"/>
			<xs:element name="Age" type="fi:Age" minOccurs="0"/>
			<xs:element name="Fecundity" type="fi:Fecundity" minOccurs="0"/>
			<xs:element name="Mortality" type="fi:Mortality" minOccurs="0"/>
			<xs:element name="BiologicalRatio" type="fi:BiologicalRatio" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BiologicalRatio" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Biological Ratio</Title>
				<Description>Biological Ratio is a container for information directly related to the Biological Ratios used to describe a species or population.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Sex"/>
					<xs:enumeration value="Length-weight"/>
					<xs:enumeration value="Length-length"/>
					<xs:enumeration value="Other_type"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Unit" type="xs:string"/>
		<xs:attributeGroup ref="fi:speciesAtt"/>
	</xs:complexType>
	<xs:complexType name="BottomType" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Bottom environment</Title>
				<Description>"Marine habitat: type of bottom substratum in which the fishery resource is distributed." (FIRMS Inventory Guidelines 2011).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Value" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Seagrass"/>
					<xs:enumeration value="Mangrove"/>
					<xs:enumeration value="Coral_reef"/>
					<xs:enumeration value="Soft_bottom"/>
					<xs:enumeration value="Soft_bottom_with_pebbles"/>
					<xs:enumeration value="Soft_bottom_with_gravel"/>
					<xs:enumeration value="Soft_bottom_gravel-sandy"/>
					<xs:enumeration value="Soft_bottom_clean_sand"/>
					<xs:enumeration value="Soft_bottom_muddy_or_muddy-sand"/>
					<xs:enumeration value="Soft_bottom_with_some_turbidity"/>
					<xs:enumeration value="Hard_bottom"/>
					<xs:enumeration value="Hard_rocky_bottom"/>
					<xs:enumeration value="Hard_bottom_with_clear_water"/>
					<xs:enumeration value="Unspecified"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="FreeValue" use="optional"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Broodstock" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<dc:Title>Broodstock</dc:Title>
				<dc:Description>contains information about aquaculture broodstock.</dc:Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Collection"/>
					<xs:enumeration value="production"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BudgetCode" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Budget Code</Title>
				<Description>contains the budget code for a FAO budget item.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BuildMaterial" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Build Material</Title>
				<Description>contains information about the kind of material used in building a vessel.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Metal"/>
					<xs:enumeration value="Wood"/>
					<xs:enumeration value="Fiberglass"/>
					<xs:enumeration value="Other"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Built" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Built</Title>
				<Description>contains elements related to the time and place that a ship was built.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="dc:Date"/>
			<xs:element name="Shipyard" type="fi:Shipyard"/>
			<xs:element ref="dc:Title"/>
			<xs:element name="LandAreaRef" type="fi:LandAreaRef"/>
			<xs:element name="Nation" type="fi:Nation"/>
			<xs:element name="City" type="fi:City"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Bycatch" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>By-catch</Title>
				<Description>"Part of a catch of a fishing unit taken incidentally in addition to the target species towards which fishing effort is directed. Some or all of it may be returned to the sea as discards, usually dead or dying. Associated species: commercial species caught with the main target ones. Discarded species: those species, including for specific life stages, released or returned to the sea, dead or alive, whether or not such organisms are brought fully on board a fishing vessel. Protected species: species (marine mammals, seabirds etc.) caught accidentally with the main target ones." (Fisheries Inventory: Method and Guidelines. FIRMS, 2007).</Description>
				<Usage>is a container for listing all the bycatch species caught  by a fishery. Use the Type attribute  to differentiate such bycatch.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="SpeciesList" type="fi:SpeciesList"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="AssociatedSpecies"/>
					<xs:enumeration value="DiscardedSpecies"/>
					<xs:enumeration value="ProtectedSpecies"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="CanBeConf" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Can be Confused</Title>
				<Description>CanBeConf is a description of why a species may be confused with another. It may instead/also contain a list of other species with which a species may be confused.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Capacity" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Capacity</Title>
				<Description>is a number representing a system's maximum sustainable output.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Unit" type="xs:string"/>
		<xs:attribute name="Value" type="xs:decimal"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="CaptureProduction" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Capture Production</Title>
				<Description>Catch and landing data by main species groups, fisheries and/or areas.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="FisheryIndicator" type="fi:FisheryIndicator"/>
			<xs:element name="FisheryIndicatorList" type="fi:FisheryIndicatorList"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="CapturedSpecies" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Captured Species</Title>
				<Description>All the species caught  by a fishery.</Description>
				<Usage>Those species considered part of the catch, according to the criteria used in the concerned fishery. These criteria would usually refer to the on board retained catch, including possible protected species. Use Target species, Associated and By-catch species to differentiate such catches.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="SpeciesList" type="fi:SpeciesList"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="CatchHandlingProcEquip" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Catch Handling and Processing Equipment</Title>
				<Description>is a container to describe the way catches are handled on board and which are the on-board processing facilities (freezer, etc).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ChapterTitle" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Chapter Title</Title>
				<Description>ChapterTitle contains the name of a chapter.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Chart" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Chart</Title>
				<Description>contains a database link to a chart.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="dc:Title" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="DKey" type="xs:string" use="required"/>
		<xs:attribute name="DKey2" use="optional"/>
		<xs:attribute name="Value" type="xs:string" use="required"/>
		<xs:attribute name="Value2" use="optional"/>
		<xs:attribute name="Ds" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="City" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>City</Title>
				<Description>City contains the name of a city used in the formation of the postal address for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Class" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Class</Title>
				<Description>Class is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" default="Taxonomic">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Taxonomic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:asfis"/>
	</xs:complexType>
	<xs:complexType name="ClassificationSystem" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>RelatedStandards</Title>
				<Description>contains the description of a classification system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="StandardRef" type="fi:StandardRef"/>
			<xs:element name="Instance" type="fi:Instance"/>
			<xs:element name="EncodingScheme" type="fi:EncodingScheme"/>
			<xs:element name="Rule" type="fi:Rule"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ClimaticZone" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Climate environment</Title>
