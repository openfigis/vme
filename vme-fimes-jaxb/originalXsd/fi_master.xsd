	<xs:complexType name="ExploitState" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Exploitation state</Title>
				<Description>Description of the state of an aquatic resource through the use of one or more standard terms.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Depleted"/>
					<xs:enumeration value="Fully exploited"/>
					<xs:enumeration value="Recovering"/>
					<xs:enumeration value="Moderately exploited"/>
					<xs:enumeration value="Overexploited"/>
					<xs:enumeration value="Uncertain"/>
					<xs:enumeration value="Underexploited"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
