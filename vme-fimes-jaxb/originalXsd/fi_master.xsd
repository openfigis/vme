			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<!--
				<xs:element ref="fi:CollectionRef"/>
				<xs:element ref="fi:LandAreaRef"/>
				<xs:element ref="fi:ManageAuthRef"/>
-->
			<xs:element name="AqResRef" type="fi:AqResRef"/>
			<xs:element name="FACPRef" type="fi:FACPRef"/>
			<xs:element name="FisheryRef" type="fi:FisheryRef"/>
			<xs:element name="FishTechniqueRef" type="fi:FishTechniqueRef"/>
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
			<xs:element name="NASORef" type="fi:NASORef"/>
			<xs:element name="OrgRef" type="fi:OrgRef"/>
			<xs:element name="SpeciesRef" type="fi:SpeciesRef"/>
			<xs:element name="TopicRef" type="fi:TopicRef"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
			<xs:element name="VMERef" type="fi:VMERef"/>
			<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="Family" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Family</Title>
				<Description>Family is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" default="Taxonomic">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Taxonomic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:asfis"/>
	</xs:complexType>
	<xs:complexType name="FAOLex" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>FAOLex</Title>
				<Description>Allows to build queries towards the FAOLex database.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="FAOLEX_ID"/>
		<xs:attribute name="Region">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="NORTH AMERICA"/>
					<xs:enumeration value="NORTH PACIFIC REGION"/>
					<xs:enumeration value="SAHEL"/>
					<xs:enumeration value="SOUTH AMERICA"/>
					<xs:enumeration value="SOUTH ASIA"/>
					<xs:enumeration value="SOUTH PACIFIC REGION"/>
					<xs:enumeration value="SOUTH-EAST ASIA"/>
					<xs:enumeration value="SOUTHERN AFRICA"/>
					<xs:enumeration value="WEST AFRICA"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:countryEnt"/>
		<xs:attribute name="Territorial_subdivision"/>
		<xs:attribute name="Basin"/>
		<xs:attribute name="Year">
			<xs:simpleType>
				<xs:restriction base="xs:string">
