				<xs:element name="PostHarvest" type="fi:PostHarvest"/>
				<xs:element name="SectorSocioEcoContribution" type="fi:SectorSocioEcoContribution"/>
				<xs:element name="StatusTrends" type="fi:StatusTrends"/>
				<xs:element name="OrgsInvolved" type="fi:OrgsInvolved"/>
				<xs:element name="LegalFrameworkOverview" type="fi:LegalFrameworkOverview"/>
				<xs:element name="AddInfo" type="fi:AddInfo"/>
				<xs:element name="EIMS" type="fi:EIMS"/>
				<xs:element name="NEMS" type="fi:NEMS"/>
				<xs:element name="FAOLex" type="fi:FAOLex"/>
