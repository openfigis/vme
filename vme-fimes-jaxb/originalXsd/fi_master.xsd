				<xs:element name="GeartypeHandlingEquip" type="fi:GeartypeHandlingEquip"/>
				<xs:element name="VesseltypeOverview" type="fi:VesseltypeOverview"/>
				<xs:element name="FishTechniqueOverview" type="fi:FishTechniqueOverview"/>
				<xs:element name="FishEquipOverview" type="fi:FishEquipOverview"/>
				<xs:element name="FishOperation" type="fi:FishOperation"/>
				<xs:element name="TargetSpecies" type="fi:TargetSpecies"/>
				<xs:element name="WaterAreaOverview" type="fi:WaterAreaOverview"/>
				<xs:element name="GeartypeEnviron" type="fi:GeartypeEnviron"/>
				<xs:element name="Costs" type="fi:Costs"/>
				<xs:group ref="fi:indexEnt" minOccurs="0"/>
			</xs:choice>
