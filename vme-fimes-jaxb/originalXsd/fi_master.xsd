			<xs:documentation>
				<Title>FI-Topic Trend entry</Title>
				<Description>contains general text and low-level elements for a Fisheries trend.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:sectorEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="StatusValue"/>
		<xs:attribute name="TrendValue"/>
	</xs:complexType>
	<xs:complexType name="StatusTrends" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Trends, Issues and Development</Title>
				<Description>Overview of the status and trends of the fisheries sector, highlighting the main issues faced and to be addressed. Attention will in particular be provided on the state of resources and supply and demand (for next 25 years). If desired, use a new fi:StatusTrendEntry element for each aspect.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="StatusTrendEntry" type="fi:StatusTrendEntry"/>
			<xs:element name="Issues" type="fi:Issues"/>
			<xs:element name="Strategies" type="fi:Strategies"/>
			<xs:element name="Constraints" type="fi:Constraints"/>
			<xs:element name="ResearchEducation" type="fi:Research"/>
			<xs:element name="AidOrInvestments" type="fi:AidOrInvestments"/>
			<!-- 2 different element names byt same type (similar to relactivity, reldoc etc. -->
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="StatusValue"/>
		<xs:attribute name="TrendValue"/>
	</xs:complexType>
	<xs:complexType name="StorageMethod" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Storage Method</Title>
				<Description>contains information about a vessel's method of storing catch.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Strategies" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Management strategies</Title>
				<Description>Management Strategies are strategies related to missions, outputs, projects, etc. Use one instance of fi:Strategy for each instance of a Strategy.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="StrategyEntry" type="fi:StrategyEntry"/>
			<xs:element name="RelatedFisheries" type="fi:RelatedFisheries"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:sectorEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="StrategyEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Strategy entry</Title>
				<Description>Use one fi:StrategyEntry for each instance of a strategy.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="SubFamily" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Subfamily</Title>
				<Description>SUBFAM is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" default="Taxonomic">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Taxonomic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:asfis"/>
	</xs:complexType>
	<xs:complexType name="SubOrder" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Sub-order</Title>
				<Description>SUBORD is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" default="Taxonomic">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Taxonomic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:asfis"/>
		<xs:attribute name="OtherType" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Subset" mixed="false">
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Key" type="fi:Key"/>
			<xs:element name="Dimension" type="fi:Dimension"/>
			<xs:element name="DataSource" type="fi:DataSource"/>
			<xs:element name="QualityAssurance" type="fi:QualityAssurance"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="SupplyAndDemand" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Supply and Utilisation</Title>
				<Description>contains supply and demand information.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="SupportIndustry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Supporting Industries</Title>
				<Description>contains information about industries that support aquaculture production.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="SurfaceArea" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Surface Area</Title>
				<Description>SurfaceArea is the surface area of an object. Tag the value and define the fi:Unit attribute.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:areaUnit"/>
		<xs:attribute name="Value" type="xs:positiveInteger"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Synonym" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Synonym</Title>
				<Description>Synonym is a container for the scientific names that exist for a species but are no longer valid. Use fi:SynEntry for each instance.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="SynonymEntry" type="fi:SynonymEntry"/>
		</xs:choice>
		<xs:attribute name="Type" default="S">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="S"/>
					<xs:enumeration value="O"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="SynonymEntry" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Synonym entry</Title>
				<Description>SynonymEntry contains a species synonym together with any accompanying notes.</Description>
				<usage>Use dwc:Taxon and its sub elements, note should go in "namePublishedIn" if referring to the original scientific name otherwise in fi:Text for any other type of information</usage>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="dwc:Taxon" minOccurs="0"/>
			<xs:group ref="fi:textEnt" minOccurs="0"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Table" mixed="true">
		<xs:sequence>
			<xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Title" type="xs:string"/>
		<xs:attribute name="ID" type="xs:string"/>
		<xs:attribute name="IDRef" type="xs:string"/>
		<xs:attribute name="DisplayType">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Inline"/>
					<xs:enumeration value="Link"/>
					<xs:enumeration value="Popup"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Tag" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Tag</Title>
				<Description>A tag is a non-hierarchical keyword or term assigned to a piece of information. This kind of metadata helps describe an item and allows it to be found again by browsing or searching.</Description>
				<Usage>Select a fixed Value among the controlled list or submit a free value. Tag should be always in capital.</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Value" use="required"/>
		<xs:attribute ref="xml:lang"/>
	</xs:complexType>
	<xs:complexType name="Tags" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Tags</Title>
				<Description>A container for tags used to describe a FIGIS Document.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Tag" type="fi:Tag" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TargetSpecies" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Target Species</Title>
				<Description>Those species that are primarily sought by the fishermen in a particular fishery. The subject of directed fishing effort in a fishery. There may be primary as well as secondary target species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="SpeciesList" type="fi:SpeciesList"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TargetSpeciesEnviron" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Fishing Technique Target Species and its Environment</Title>
				<Description>TargetSpeciesEnviron contains information about a target species and its environment.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="TargetSpecies" type="fi:TargetSpecies"/>
			<xs:element name="FishingEnviron" type="fi:FishingEnviron"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Telephone" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Telephone</Title>
				<Description>Telephone contains the telephone number for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="CountryPrefix" type="xs:positiveInteger"/>
		<xs:attribute name="AreaPrefix" type="xs:positiveInteger"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Telex" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Telex</Title>
				<Description>Telex contains the telex number for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TermsAndConditions" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>TermsAndConditions</Title>
				<Description>contains information about contractual requirements related to the authorisation to engage in aquaculture and/or fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Text" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Text block</Title>
				<Description>Text container, which might include an optional Title, as well as HTML formatting tags within the CDATA text string element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Title" type="xs:string"/>
		<xs:attribute ref="xml:lang"/>
	</xs:complexType>
	<xs:complexType name="ThreatToHumans" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Threat to Humans</Title>
				<Description>ThreatToHumans is a review of the possible aggressive behaviour of a species toward humans.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TimeRange" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Time Range</Title>
				<Description>XXXXX</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="Range" type="fi:Range"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Tonnage" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Tonnage</Title>
				<Description>Tonnage is a measurement of a vessel's capacity. The gross tonnage is the overall volume of a ship's hull, including crew cabins, storerooms and machinery spaces. GRT is the gross registered tonnage of the vessel as entered on the national reigster or other record of vessels.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Net"/>
					<xs:enumeration value="Gross"/>
					<xs:enumeration value="GRT"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Unit" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Register_tons"/>
					<xs:enumeration value="Metric_tons"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Value" type="xs:positiveInteger"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Topic" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Topic</Title>
				<Description>This is the root element for the domain and contains three levels of complexity:Select the Text container for lightweight description. It allows for text, formatting, and simple elements related to the domain.Select the Ref container for positive linking to an existing database object. This will cause the FIGIS database loader to link to the existing object so that any description in the document will be complemented by the existing information in the database, though it will NOT alter the existing contents of the referenced database object.Select Ident only when the goal is to create a new database object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TopicIdent" type="fi:TopicIdent"/>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Overview" type="fi:Overview"/>
				<xs:element name="Issues" type="fi:Issues"/>
				<xs:element name="TopicImages" type="fi:TopicImages"/>
				<xs:group ref="fi:indexEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="CTN" type="xs:positiveInteger"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TopicCoverage" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Topic Coverage</Title>
				<Description>the topics with which an organisation is concerned.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="TopicRef" type="fi:TopicRef"/>
			<xs:element name="TopicCoverageType" type="fi:TopicCoverageType"/>
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TopicCoverageType" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Topic Coverage Type</Title>
				<Description>the type of the topic coverage.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Aquaculture"/>
					<xs:enumeration value="Conservation"/>
					<xs:enumeration value="Environment"/>
					<xs:enumeration value="GIS/Remote_sensing"/>
					<xs:enumeration value="Inland_fisheries"/>
					<xs:enumeration value="Marine_fisheries"/>
					<xs:enumeration value="Recreational_fisheries"/>
					<xs:enumeration value="Social_issues"/>
					<xs:enumeration value="Aquaculture statistics"/>
					<xs:enumeration value="Artisanal_fisheries"/>
					<xs:enumeration value="Capture_fisheries"/>
					<xs:enumeration value="Ecosystem_approach"/>
					<xs:enumeration value="Fisheries_governance"/>
					<xs:enumeration value="Fisheries_statistics"/>
					<xs:enumeration value="Responsible_fisheries"/>
					<xs:enumeration value="Small-scale_fisheries"/>
					<xs:enumeration value="Trade"/>
					<xs:enumeration value="Utilization"/>
					<!-- the following are added by NEMS KW to be cleared and then updated in RTMS -->
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TopicFeature" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Topic Feature</Title>
				<Description>is a container for supplementary elements related to the object but not essential to its Identification or distinction, e.g. for an aquatic species this would be elements such as conservation status or interest to fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Sources" type="fi:Sources"/>
			<xs:element name="RelatedResources" type="fi:RelatedResources"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TopicIdent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Topic Identity</Title>
				<Description>Ident contains the elements needed to positively and uniquely Identify a domain object. Inclusion of this element in a document causes the FIGIS database loader to create a database object that will be keyed using the information found in the Ident element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="FigisID" type="fi:FigisID"/>
			<xs:sequence>
				<xs:element ref="dc:Title" maxOccurs="6"/>
				<xs:element name="TopicParent" type="fi:TopicParent" minOccurs="0"/>
				<xs:element name="ForeignID" type="fi:ForeignID" minOccurs="0"/>
			</xs:sequence>
		</xs:choice>
		<xs:attributeGroup ref="fi:statusEnt"/>
		<xs:attribute ref="xml:lang" use="optional"/>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Topic"/>
					<xs:enumeration value="Issue"/>
					<xs:enumeration value="Gateway"/>
					<xs:enumeration value="Activity"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TopicImages" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Images</Title>
				<Description>can hold text and/or other images for a gallery.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TopicParent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Topic Parent</Title>
				<Description>is a container for the reference to a topic parent.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TopicRef" type="fi:TopicRef"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TopicProfile" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Fisheries Topic Profile</Title>
				<Description>Profile contains the elements used by domain specialists to profile a domain object and declare it as a unique type. That is, rather than containing the Identifying codes or names (Keys) for the object as in Ident, the Profile block contains the information that is intrinsic and invariable to a description of the object, e.g. for an aquatic species this would be elements such as fieldmarks or diagnostic features. </Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TopicRef" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>FI-Topic reference</Title>
				<Description>contains positive ID References to the fi-topic domain.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="FigisID" type="fi:FigisID"/>
			<xs:element ref="dc:Title"/>
			<xs:element name="ForeignID" type="fi:ForeignID"/>
			<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="Trade" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Trade</Title>
				<Description>contains information about fishing trade. Use fi:Tradeflows for import/export text.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="Tradeflows" type="fi:Tradeflows"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="TradeflowEntry" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Tradeflow entry</Title>
				<Description>contains information about fishing trade flows. Use the Type attribute to delimit the tag.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="EndMarket" type="fi:EndMarket"/>
			<xs:element name="Origin" type="fi:Origin"/>
		</xs:choice>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Volume"/>
					<xs:enumeration value="Value"/>
					<xs:enumeration value="Volume_and_value"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Tradeflows" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Tradeflows</Title>
				<Description>contains information about fishing trade flows. Use fi:TradeflowEntry for individual tradeflows.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="TradeflowEntry" type="fi:TradeflowEntry"/>
		</xs:choice>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Import"/>
					<xs:enumeration value="Export"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Tribe" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Tribe</Title>
				<Description>Tribe is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" default="Taxonomic">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Taxonomic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:asfis"/>
	</xs:complexType>
	<xs:complexType name="TripDuration" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Trip duration</Title>
				<Description>"Average duration of fishing trip specifying in which unit it is expressed" (Fisheries Inventory: Method and Guidelines. FIRMS, 2007).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TypeOfProductionSystem" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Type of Production System</Title>
				<Description>the scale at which a fishery operates.</Description>
				<Usage>is a container for indicating the scale at which a fishery operates (e.g. Artisanal-Small Scale, Commercial- Small Scale, Semi industrial).</Usage>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="FreeValue" use="optional"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attribute name="Class">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Large-scale"/>
					<xs:enumeration value="Small-scale"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:sectorEnt"/>
		<xs:attribute name="Value">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Subsistence"/>
					<xs:enumeration value="Recreational"/>
					<xs:enumeration value="Commercial"/>
					<xs:enumeration value="Artisanal"/>
					<xs:enumeration value="Semi-industrial"/>
					<xs:enumeration value="Industrial"/>
					<xs:enumeration value="Exploratory_fishery"/>
					<xs:enumeration value="Unspecified"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TypicalUsage" mixed="false">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Uncertainty" mixed="false">
		<xs:attribute name="Value" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="High"/>
					<xs:enumeration value="Intermediate"/>
					<xs:enumeration value="Low"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Unit" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Unit</Title>
				<Description>contains the name of a measurement unit.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ValidationMechanism" mixed="false">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Value" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Value</Title>
				<Description>contains a value.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="Range" type="fi:Range"/>
			<xs:element name="Avg" type="fi:Avg"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VerticalDist" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Vertical distribution</Title>
				<Description>"Marine habitat: type of habitat describing the fishery resource distribution according to its relationship with the sea bottom." (FIRMS Inventory Guidelines 2011).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang" use="optional"/>
		<xs:attribute name="Value" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Demersal/Benthic"/>
					<xs:enumeration value="Demersal"/>
					<xs:enumeration value="Benthic"/>
					<xs:enumeration value="Pelagic"/>
					<xs:enumeration value="Unspecified"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="FreeValue" use="optional"/>
	</xs:complexType>
	<xs:complexType name="VesselActive" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Active</Title>
				<Description>is a container for registry status information on active vessels.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute name="DateAuthorised" type="xs:date"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselOperator" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Operator</Title>
				<Description>contains the name and/or contact information for a vessel operator. It means the owner of bareboat charterer, or any other natural or juridical person to whom the responsibilities of the owner or bareboat charterer have been formally assigned</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselOwner" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Owner</Title>
				<Description>contains the name and/or contact information for a vessel owner. It means any natural or juridical person recorded in the register of ships of the State of registration as an owner of a shipcontains the name and/or contact information for a vessel owner.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselRecord" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord</Title>
				<Description>This is the root element for the domain and contains three levels of complexity:Select the Text container for lightweight description. It allows for text, formatting, and simple elements related to the domain.Select the Ref container for positive linking to an existing database object. This will cause the FIGIS database loader to link to the existing object so that any description in the document will be complemented by the existing information in the database, though it will NOT alter the existing contents of the referenced database object.Select Ident only when the goal is to create a new database object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VesselRecordIdent" type="fi:VesselRecordIdent"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="VesselRecordProfile" type="fi:VesselRecordProfile"/>
				<xs:element name="VesselRecordFeature" type="fi:VesselRecordFeature"/>
			</xs:choice>
			<xs:group ref="fi:indexEnt" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselRecordFeature" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord Feature</Title>
				<Description>is a container for supplementary elements related to the object but not essential to its Identification or distinction, e.g. for an aquatic species this would be elements such as conservation status or interest to fisheries.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="VesselReport" type="fi:VesselReport"/>
			<xs:element name="VesselRegistryStatus" type="fi:VesselRegistryStatus"/>
			<xs:element name="Infringement" type="fi:Infringement"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselRecordIdent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord Identity</Title>
				<Description>Ident contains the elements needed to positively and uniquely Identify a domain object. Inclusion of this element in a document causes the FIGIS database loader to create a database object that will be keyed using the information found in the Ident element.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="FigisID" type="fi:FigisID"/>
			<xs:sequence maxOccurs="unbounded">
				<xs:element name="VesselRecordIntID" type="fi:VesselRecordIntID" maxOccurs="unbounded"/>
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="Built" type="fi:Built"/>
					<xs:element name="Image" type="fi:Image"/>
				</xs:choice>
			</xs:sequence>
		</xs:choice>
		<xs:attributeGroup ref="fi:statusEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselRecordIntID" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord International Id</Title>
				<Description>contains international classification codes and related information for VesselRecord Identification.IMOID is the vessel Identifying code assigned by Lloyd's Register of Shipping, a classification society based in the United Kingdom. Permanent number assigned to ships for Identification purposes. It is made of the three letters IMO in front of the Lloyd's Register of Shipping (Lr) Number.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IMOConfidence" type="fi:IMOConfidence" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="CodeSystem" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="HSVAR"/>
					<xs:enumeration value="IMO"/>
					<xs:enumeration value="InMarSat"/>
					<xs:enumeration value="RFB"/>
					<xs:enumeration value="EU"/>
					<xs:enumeration value="Other"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Source" type="xs:string"/>
		<xs:attributeGroup ref="fi:code"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselRecordNatID" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord National Id</Title>
				<Description>contains the Identifying vessel's codes valid at a national or regional, but not global, level. These codes are subject to changes if the vessel operates under different flag.Callsign contains the Identifying code letters or numbers used for Identification during radio transmission. This is a unique identifier allocated to each ship by its national competent authority. Each country is allocated, by the International telecommunication union (Itu), codes beginning with specific characters. The first two characters are two letters or a letter followed by a digit or a digit followed by a letter. The first two characters or in certain cases the first character of a call sign constitute the nationality Identification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="fi:code"/>
		<xs:attribute name="CodeSystem" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Name"/>
					<xs:enumeration value="Callsign"/>
					<xs:enumeration value="RegNumber"/>
					<xs:enumeration value="Flagstate"/>
					<xs:enumeration value="Other"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselRecordProfile" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord Profile</Title>
				<Description>Profile contains the elements used by domain specialists to profile a domain object and declare it as a unique type. That is, rather than containing the Identifying codes or names (Keys) for the object as in Ident, the Profile block contains the information that is intrinsic and invariable to a description of the object, e.g. for an aquatic species this would be elements such as fieldmarks or diagnostic features. </Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="VesselSpecs" type="fi:VesselSpecs"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselRecordRef" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselRecord reference</Title>
				<Description>is used to Reference a unique vessel NOT a vesseltype.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:element name="ForeignID" type="fi:ForeignID"/>
			</xs:choice>
			<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VesselRegistryStatus" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>High Seas Vessel Registry Status</Title>
				<Description>VESSELREGISTRY is a container for registry status information.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="dc:Date"/>
			<xs:element name="Sources" type="fi:Sources"/>
			<xs:choice>
				<xs:element name="VesselActive" type="fi:VesselActive"/>
				<xs:element name="VesselSuspended" type="fi:VesselSuspended"/>
			</xs:choice>
			<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselReport" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Report</Title>
				<Description>is a container for descriptive elements related to a Vessel which are subject to change.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="dc:Date"/>
			<xs:element name="VesselRecordNatID" type="fi:VesselRecordNatID"/>
			<xs:element name="PortOfCall" type="fi:PortOfCall"/>
			<xs:element name="Province" type="fi:Province"/>
			<xs:element name="VesselOwner" type="fi:VesselOwner"/>
			<xs:element name="VesselOperator" type="fi:VesselOperator"/>
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
			<xs:element name="StorageMethod" type="fi:StorageMethod"/>
			<xs:element name="WheelhEquip" type="fi:WheelhEquip"/>
			<xs:element name="Highlight" type="fi:Highlight"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselSold" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Sold</Title>
				<Description>is a container for registry status information for vessels that have been sold.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="LandAreaRef" type="fi:LandAreaRef"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselSpecs" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Technical Specifications</Title>
				<Description>contains a group of elements detailing the technical specifications of a Vessel.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="VesseltypeLength" type="fi:VesseltypeLength"/>
			<xs:element name="VesseltypeDepth" type="fi:VesseltypeDepth"/>
			<xs:element name="Draught" type="fi:Draught"/>
			<xs:element name="Beam" type="fi:Beam"/>
			<xs:element name="PowerClass" type="fi:PowerClass"/>
			<xs:element name="BuildMaterial" type="fi:BuildMaterial"/>
			<xs:element name="Tonnage" type="fi:Tonnage"/>
			<xs:element name="HoldCapacity" type="fi:HoldCapacity"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselSuspended" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel Suspended</Title>
				<Description>is a container for registry status information on suspended vessels.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="VesselSold" type="fi:VesselSold"/>
		</xs:choice>
		<xs:attribute name="DateAuthorised" type="xs:date"/>
		<xs:attribute name="DateSuspended" type="xs:date"/>
		<xs:attribute name="ReasonSuspended" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Voluntary_relinquishment_or_non-renewal_of_the_fishing_authorization_by_the_fishing_vessel_owner_or_operator"/>
					<xs:enumeration value="Withdrawal_of_the_fishing_authorization_issued_in_respect_of_the_fishing_vessel_under_paragraph_8_of_articleIii"/>
					<xs:enumeration value="Fishing_vessel_concerned_is_no_longer_entitled_to_fly_its_flag"/>
					<xs:enumeration value="Scrapping_decommissioning_or_loss_of_the_fishing_vessel_concerned"/>
					<xs:enumeration value="Any_other_reason"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Vesseltype" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vesseltype</Title>
				<Description>This is the root element for the domain. It allows for text, formatting, and simple elements related to the domain. Select the Ref container for positive linking to an existing database object. This will cause the FIGIS database loader to link to the existing object so that any description in the document will be complemented by the existing information in the database, though it will NOT alter the existing contents of the referenced database object. Select Ident only when the goal is to create a new database object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VesseltypeIdent" type="fi:VesseltypeIdent"/>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Overview" type="fi:Overview"/>
				<xs:element name="GeartypeOverview" type="fi:GeartypeOverview"/>
				<xs:element name="FishTechniqueOverview" type="fi:FishTechniqueOverview"/>
				<xs:element name="FishEquipOverview" type="fi:FishEquipOverview"/>
				<xs:element name="DeckType" type="fi:DeckType"/>
				<xs:element name="VesseltypeClass" type="fi:VesseltypeClass"/>
				<xs:element name="DeckArrange" type="fi:DeckArrange"/>
				<xs:element name="CatchHandlingProcEquip" type="fi:CatchHandlingProcEquip"/>
				<xs:element name="VesseltypeEquip" type="fi:VesseltypeEquip"/>
				<xs:group ref="fi:indexEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesseltypeClass" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesseltypeClass</Title>
				<Description>VesseltypeClass is a vessel classification grouping.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="VesseltypeLength" type="fi:VesseltypeLength"/>
			<xs:element name="PowerClass" type="fi:PowerClass"/>
			<xs:element name="Tonnage" type="fi:Tonnage"/>
			<xs:element name="AddInfo" type="fi:AddInfo"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesseltypeDepth" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vesseltype Depth</Title>
				<Description>VesseltypeDepth is a measurement from the underside of a vessel's deck to the bottom of its hold.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attributeGroup ref="fi:lengthUnit"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesseltypeEquip" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Equipment</Title>
				<Description>VesseltypeEquip is a container for vessel equipment elements.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="DeckEquip" type="fi:DeckEquip"/>
			<xs:element name="WheelhEquip" type="fi:WheelhEquip"/>
			<xs:element name="FishDetEquip" type="fi:FishDetEquip"/>
			<xs:element name="GeartypeOverview" type="fi:GeartypeOverview"/>
			<xs:element name="CatchHandlingProcEquip" type="fi:CatchHandlingProcEquip"/>
			<xs:element name="SecuEquip" type="fi:SecuEquip"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesseltypeIdent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vesseltype Identity</Title>
				<Description>VesseltypeIdent contains the core classification codes and related information for positive vessel type identification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="FigisID" type="fi:FigisID"/>
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element name="ISSCFVCode" type="fi:ISSCFVCode"/>
					<xs:element name="FIDICode" type="fi:FIDICode"/>
				</xs:choice>
				<xs:element ref="dc:Title" maxOccurs="3"/>
				<xs:choice maxOccurs="unbounded">
					<xs:element name="Image" type="fi:Image" minOccurs="0"/>
					<xs:element ref="dc:Description"/>
				</xs:choice>
			</xs:sequence>
		</xs:choice>
		<xs:attributeGroup ref="fi:statusEnt"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesseltypeLength" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>VesseltypeLength</Title>
				<Description>is a measurement of a boat's longest dimension. For purposes of the HSVAR agreement the attribute ComplianceType has been added. TYPE 1 for any fishing vessel built after 18 July 1982, 96 percent of the total length on a waterline at 85 percent of the least moulded depth measured from the top of the keel, or the length from the foreside of the stern to the axis of the rudder stock on that waterline, if that be greater. In ships designed with a rake of keel the waterline on which this length is measured shall be parallel to the designed waterline TYPE 2 For any fishing vessel built before 18 July 1982, registered length as entered on the national register or other record of vessels</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="LOA"/>
					<xs:enumeration value="Actual"/>
					<xs:enumeration value="Registered"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attributeGroup ref="fi:lengthUnit"/>
		<xs:attribute name="ComplianceType">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Pre82"/>
					<xs:enumeration value="Post82"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesseltypeOverview" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel type overview</Title>
				<Description>VesseltypeOverview contains high-level information about one instance of one vesseltype. Use a new element for each vesseltype.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="GeartypeRef" type="fi:GeartypeRef"/>
			<xs:element name="VesseltypeRef" type="fi:VesseltypeRef"/>
			<xs:element name="FishTechniqueRef" type="fi:FishTechniqueRef"/>
			<xs:element name="FlagStateList" type="fi:FlagStateList"/>
			<xs:element name="VesseltypeClass" type="fi:VesseltypeClass"/>
			<!--<xs:element ref="fi:Tonnage"/> removed as it is better to have all classes of a vessels (tonnage, length..) hence it is substituted with VesseltypeClass, decided on 12-03-2008, to update vesseltype fact sheets (REF_ID : 280, 735, 738, 350, 270, 100, 290, 110) when the schema will be updated to 3.6. -->
			<xs:element name="Crew" type="fi:Crew"/>
			<xs:element name="CatchHandlingProcEquip" type="fi:CatchHandlingProcEquip"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesseltypeRef" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel type reference</Title>
				<Description>is used to positively identify vessel types. ISSSCFV or FIES code is required. Select a classification system, and enter the value in 'Code'.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:element name="ForeignID" type="fi:ForeignID"/>
			</xs:choice>
			<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6"/>
			<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Vesseltypes" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Vessel types</Title>
				<Description>Vesseltypes contains high-level information about vesseltypes. Use a new Entry element for each vesseltype instance.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="VesseltypeOverview" type="fi:VesseltypeOverview"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselUnderJurisdiction" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselUnderJurisdiction</Title>
				<Description>contains information about the jurisdiction on which the vessel is targeted by a measure.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="VesselUnderJurisdictionType" type="fi:VesselUnderJurisdictionType"/>
			<xs:element name="FlagStateList" type="fi:FlagStateList"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VesselUnderJurisdictionType" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>VesselUnderJurisdictionType</Title>
				<Description>"The flag or status of the vessel targeted by the measure" (LEGN, 2008).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="FlagStateList" type="fi:FlagStateList"/>
			<xs:element name="OrgRef" type="fi:OrgRef"/>
		</xs:choice>
		<xs:attribute name="Value" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="All_vessels"/>
					<xs:enumeration value="National_vessels"/>
					<xs:enumeration value="Foreign_vessels"/>
					<xs:enumeration value="Foreign_flag"/>
					<xs:enumeration value="RFMO"/>
					<xs:enumeration value="Non-RFMO"/>
					<xs:enumeration value="EU_vessels"/>
					<xs:enumeration value="Non-EU_vess"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang" use="optional"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Volume" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Volume</Title>
				<Description>contains a measurement. It may be by weight or by volume (Space). Define the correct fi:Unit attribute and tag the value. Use Scale if necessary, e.g. if the text reads '20 million' tag '20' as a value and insert 'million' in the Scale attribute.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="fi:volumeUnit"/>
		<xs:attributeGroup ref="fi:monetaryScale"/>
		<xs:attribute name="Value" type="xs:positiveInteger"/>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VolumeTitle" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Volume Title</Title>
				<Description>VolumeTitle contains the name or number of a volume.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="WaterAreaList" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Water Area List</Title>
				<Description>contains a group of water area references with optionally an ad hoc title for the corresponding group.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element ref="dc:Title"/>
			<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="WaterAreaOverview" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Water area overview</Title>
				<Description>"Description of the water area in which the aquatic resource is located, including environmental characteristics, jurisdictional limits, ..." (FIGIS-FIRMS 2002 workshop). Water Area Overview might contain a reference list of areas in which a species or aquatic resource is found or employed.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="SpatialScale" type="fi:SpatialScale" minOccurs="0"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="WaterAreaRef" type="fi:WaterAreaRef"/>
				<xs:element name="History" type="fi:History"/>
				<xs:group ref="fi:textEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="WaterAreaRef" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Water area reference</Title>
				<Description>Element used to refer to a Water Area domain element. It contains water geographic systems and codes (FAO Area, Five Degree Square, ISO country, regional/local codes, water competence zones and environments, ...) used for positive referencing of a water area.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:element name="ForeignID" type="fi:ForeignID"/>
			</xs:choice>
			<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6"/>
			<xs:element name="IntersectingAreas" type="fi:IntersectingAreas" minOccurs="0"/>
			<xs:element name="AdditionalRefData" type="fi:AdditionalRefData" minOccurs="0"/>
			<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Type">
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="Environmental"/>
					<xs:enumeration value="Statistical"/>
					<xs:enumeration value="Competence"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="WaterCondition" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Water Conditions</Title>
				<Description>WaterCondition is information on species affinity related to water conditions or characteristics , e.g. oxygen , temperature etc.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="WaterQuality" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>WaterQuality</Title>
				<Description>contains information about WaterQuality for aquaculture.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="License" type="fi:License"/>
			<xs:element name="OrgsInvolved" type="fi:OrgsInvolved"/>
			<xs:element name="TermsAndConditions" type="fi:TermsAndConditions"/>
			<xs:element name="Procedures" type="fi:Procedures"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="WaterTemp" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Depth Range</Title>
				<Description>contains a minimum through maximum depth range.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute name="MinCelsius">
			<xs:simpleType>
				<xs:restriction base="xs:nonNegativeInteger">
					<xs:pattern value="[0-9][0-9]?"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="MaxCelsius">
			<xs:simpleType>
				<xs:restriction base="xs:nonNegativeInteger">
					<xs:pattern value="[0-9][0-9]?"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Website" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Website</Title>
				<Description>Website contains the WWW ADDRESS for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Weight" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Weight</Title>
				<Description>Weight is a container for information directly related to the weight of a species.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="Min" type="fi:Min"/>
			<xs:element name="Max" type="fi:Max"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
		<xs:attributeGroup ref="fi:ageType"/>
		<xs:attributeGroup ref="fi:weightUnit"/>
		<xs:attributeGroup ref="fi:speciesAtt"/>
	</xs:complexType>
	<xs:complexType name="WheelhEquip" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>Wheel Equipment</Title>
				<Description>WheelhEquip is a vessel's wheelhouse equipment.Contains information on all the equipment present in the wheelhouse. An enclosed area, usually on the bridge of a vessel, from which the vessel is controlled when under way.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Year" mixed="true">
		<xs:annotation>
			<xs:documentation>
				<Title>Year</Title>
				<Description>Year contains the four digit number for a year. "The date a measure was adopted by the relevant authority" (LEGN, 2008).</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VME" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VME</Title>
				<Description>Vulnerable Marine Ecosystem (VME)</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VMEIdent" type="fi:VMEIdent"/>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Overview" type="fi:Overview"/>
				<xs:element name="VMERef" type="fi:VMERef"/>
				<xs:group ref="fi:indexEnt"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="VMEIdent" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VME identity</Title>
				<Description>VMEIdent contains identity information about VME.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="FigisID" type="fi:FigisID"/>
				<xs:element name="ForeignID" type="fi:ForeignID" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="dc:Title"/>
				<xs:element name="ReportingYear" type="fi:ReportingYear" maxOccurs="1"/>
			<xs:element name="AdditionalRefData" type="fi:AdditionalRefData" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
		<xs:attributeGroup ref="fi:statusEnt"/>
	</xs:complexType>
	<xs:complexType name="VMERef" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>VME reference</Title>
				<Description> Element used to Refer to a VME record.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="FigisID" type="fi:FigisID" maxOccurs="1"/>
			<xs:element ref="dc:Title" maxOccurs="6"/>
			<xs:element name="ForeignID" type="fi:ForeignID" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
</xs:schema>
