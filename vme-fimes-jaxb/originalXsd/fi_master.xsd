					<xs:enumeration value="Zoning"/>
					<xs:enumeration value="Early_warning"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ControlSystems" mixed="false">
		<xs:annotation>
			<xs:documentation>
				<Title>ControlSystems</Title>
				<Description>contains information about disease control systems.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:group ref="fi:textEnt"/>
			<xs:element name="ControlSystemEntry" type="fi:ControlSystemEntry"/>
		</xs:choice>
		<xs:attribute ref="xml:lang"/>
		<xs:attribute name="Label" type="xs:string"/>
	</xs:complexType>
