			</xs:documentation>
		</xs:annotation>
		<xs:choice maxOccurs="unbounded">
			<xs:element name="OfficialDate" type="fi:OfficialDate"/>
			<xs:element name="ConsolidatedDate" type="fi:ConsolidatedDate"/>
