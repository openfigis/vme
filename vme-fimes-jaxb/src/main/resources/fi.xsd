	<xs:element name="Email">
		<xs:annotation>
			<xs:documentation>
				<Title>E-mail</Title>
				<Description>Email contains the email ADDRESS for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="MailTo" type="xs:anyURI" use="required"/>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Employment">
		<xs:annotation>
			<xs:documentation>
				<Title>Employment</Title>
				<Description>is a container for one or more fi:EmploymentEntry elements.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
				<xs:element ref="fi:FisheryIndicator"/>
			</xs:choice>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="EncodingScheme">
		<xs:annotation>
			<xs:documentation>
				<Title>RelatedStandards</Title>
				<Description>contains a description of an encoding scheme used by a classification system.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
				<xs:element ref="fi:Rule"/>
			</xs:choice>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="EndMarket">
		<xs:annotation>
			<xs:documentation>
				<Title>End markets</Title>
				<Description>is a container for lists of countries that are the destination markets for a fisheries product or products.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
			</xs:choice>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="EnvironAssessment">
		<xs:annotation>
			<xs:documentation>
				<Title>EnvironAssessment</Title>
				<Description>contains information about environmental impact assessments for aquaculture.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
				<xs:element ref="fi:TermsAndConditions"/>
				<xs:element ref="fi:Procedures"/>
				<xs:element ref="fi:Guidelines"/>
				<xs:element ref="fi:OrgsInvolved"/>
				<xs:element ref="fi:ManageSystemOverview"/>
			</xs:choice>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="EstablishedWild">
		<xs:annotation>
			<xs:documentation>
				<Title>Established in Wildr </Title>
				<Description>Element contains information on weather the species is established in the Wild after his IntroducedAqSpecies. If this is the case can be specified how this has been achieved.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Status" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Yes"/>
						<xs:enumeration value="Probably_yes"/>
						<xs:enumeration value="No"/>
						<xs:enumeration value="Probably_no"/>
						<xs:enumeration value="Unknown"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Trough">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Natural_reproduction"/>
						<xs:enumeration value="Continuous_restocking"/>
						<xs:enumeration value="Natural_reproduction_continuos_restocking"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Exploit">
		<xs:annotation>
			<xs:documentation>
				<Title>Exploitation</Title>
				<Description>Information about "the exploitation of the aquatic resource, including typically the description of the various fisheries exploiting the stock, and exploitation indicators such as catches, effort parameters, size of fish caught ..." (FIGIS-FIRMS 2002 workshop). Use fi:FisheryRef to positively reference the fisheries exploiting this aquatic resource.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
				<xs:element ref="fi:FisheryRef"/>
				<xs:element ref="fi:History" minOccurs="0"/>
			</xs:choice>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExploitRate">
		<xs:annotation>
			<xs:documentation>
				<Title>Exploitation Rate</Title>
				<Description>"The proportion of a population at the beginning of a given time period that is caught during that time period (usually expressed on a yearly basis)." (FAO Fisheries Glossary)</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:StandardExploitRate"/>
			</xs:choice>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
			<xs:attribute name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="CCAMLR"/>
						<xs:enumeration value="CCSBT"/>
						<xs:enumeration value="CECAF"/>
						<xs:enumeration value="EUROSTAT"/>
						<xs:enumeration value="FAO"/>
						<xs:enumeration value="FIRMS"/>
						<xs:enumeration value="GFCM"/>
						<xs:enumeration value="IATTC"/>
						<xs:enumeration value="ICCAT"/>
						<xs:enumeration value="ICES"/>
						<xs:enumeration value="IOTC"/>
						<xs:enumeration value="NAFO"/>
						<xs:enumeration value="NEAFC"/>
						<xs:enumeration value="SEAFDEC"/>
						<xs:enumeration value="SEAFO"/>
						<xs:enumeration value="WECAFC"/>
						<xs:enumeration value="Unspecified"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="FreeValue" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExploitState">
		<xs:annotation>
			<xs:documentation>
				<Title>Exploitation state</Title>
				<Description>Description of the state of an aquatic resource through the use of one or more standard terms.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:attribute name="Value" use="required">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Depleted"/>
						<xs:enumeration value="Fully exploited"/>
						<xs:enumeration value="Recovering"/>
						<xs:enumeration value="Moderately exploited"/>
						<xs:enumeration value="Overexploited"/>
						<xs:enumeration value="Uncertain"/>
						<xs:enumeration value="Underexploited"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FACPRef">
		<xs:annotation>
			<xs:documentation>
				<Title>Fishery and Aquaculture Profile (FACP) reference</Title>
				<Description>is the Ref container for referencing a Fishery and Aquaculture fact sheet.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:choice maxOccurs="unbounded">
					<xs:element ref="fi:FigisID"/>
					<xs:element ref="fi:ForeignID"/>
				</xs:choice>
				<xs:element ref="dc:Title" minOccurs="0" maxOccurs="6"/>
				<xs:element ref="fi:OfficialName" minOccurs="0"/>
				<xs:element ref="dc:Identifier" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Factsheet">
		<xs:annotation>
			<xs:documentation>
				<Title>Fact sheet</Title>
				<Description>contains one or more references to FIGIS fact sheets.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="fi:AqResRef"/>
				<xs:element ref="fi:FACPRef"/>
				<xs:element ref="fi:FisheryRef"/>
				<xs:element ref="fi:FishTechniqueRef"/>
				<xs:element ref="fi:GeartypeRef"/>
				<xs:element ref="fi:NASORef"/>
				<xs:element ref="fi:OrgRef"/>
				<xs:element ref="fi:SpeciesRef"/>
				<xs:element ref="fi:TopicRef"/>
				<xs:element ref="fi:VesseltypeRef"/>
				<xs:element ref="fi:VMERef"/>
				<xs:element ref="fi:WaterAreaRef"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="Family">
		<xs:annotation>
			<xs:documentation>
				<Title>Family</Title>
				<Description>Family is a level of a taxonomical classification.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="Type" default="Taxonomic">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Taxonomic"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:asfis"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FAOLex">
		<xs:annotation>
			<xs:documentation>
				<Title>FAOLex</Title>
				<Description>Allows to build queries towards the FAOLex database.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:attribute name="FAOLEX_ID"/>
			<xs:attribute name="Region">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="NORTH AMERICA"/>
						<xs:enumeration value="NORTH PACIFIC REGION"/>
						<xs:enumeration value="SAHEL"/>
						<xs:enumeration value="SOUTH AMERICA"/>
						<xs:enumeration value="SOUTH ASIA"/>
						<xs:enumeration value="SOUTH PACIFIC REGION"/>
						<xs:enumeration value="SOUTH-EAST ASIA"/>
						<xs:enumeration value="SOUTHERN AFRICA"/>
						<xs:enumeration value="WEST AFRICA"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attributeGroup ref="fi:countryEnt"/>
			<xs:attribute name="Territorial_subdivision"/>
			<xs:attribute name="Basin"/>
			<xs:attribute name="Year">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:pattern value="[0-9]{4,}"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Keywords"/>
			<xs:attribute name="Words_from_title"/>
			<xs:attribute name="Exclude_repealed_legislation">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Yes"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="Type_of_text">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="International_agreements"/>
						<xs:enumeration value="Legislation"/>
						<xs:enumeration value="Regulations"/>
						<xs:enumeration value="Miscellaneous"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="target"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FAOName">
		<xs:annotation>
			<xs:documentation>
				<Title>FAO Name</Title>
				<Description>FAOName is the full FAO name in English , French , and/or Spanish. At least one must appear.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:sequence>
				<xs:element ref="dc:Title" maxOccurs="3"/>
			</xs:sequence>
			<xs:attribute name="Type" default="Statistical">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Statistical"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FarmingSystemEntry">
		<xs:annotation>
			<xs:documentation>
				<Title>Farming System</Title>
				<Description>contains information about a single type of aquaculture farming. Include the Type and Environment if desired.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
				<xs:element ref="dc:Title"/>
				<xs:element ref="fi:Capacity"/>
				<xs:element ref="fi:Number"/>
				<xs:element ref="fi:Volume"/>
				<xs:element ref="fi:Value"/>
				<xs:element ref="fi:SurfaceArea"/>
			</xs:choice>
			<xs:attributeGroup ref="fi:byType"/>
			<xs:attributeGroup ref="fi:environEnt"/>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="FarmingSystems">
		<xs:annotation>
			<xs:documentation>
				<Title>Farming Systems</Title>
				<Description>contains information about aquaculture farming. Use an fi:FarmingSystemEntry tag for every distinct type of aquaculture farming.</Description>
			</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="false">
			<xs:choice maxOccurs="unbounded">
				<xs:group ref="fi:textEnt"/>
				<xs:element ref="fi:FarmingSystemEntry"/>
				<xs:element ref="fi:CulturedSpecies"/>
				<xs:element ref="fi:ProductionSites"/>
				<xs:element ref="fi:PostHarvest"/>
				<xs:element ref="fi:SpecialisationIntegration"/>
			</xs:choice>
			<xs:attribute ref="xml:lang"/>
			<xs:attribute name="Label" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Fax">
		<xs:annotation>
			<xs:documentation>
				<Title>Fax Number</Title>
				<Description>Fax contains the fax number for a FIGIS object.</Description>
			</xs:documentation>
		</xs:annotation>
